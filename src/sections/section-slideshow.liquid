{% include 'slideshow-snippet' %}

{% schema %}
{
  "name": "Slideshow",
  "settings": [
    {
      "type": "paragraph",
      "content": "Created by 'Slideshow' section. Please try to keep your slideshow images the same size."
    },
    {
      "id": "title",
      "type": "text",
      "label": "Section Label",
      "default": "Slideshow"
    },
    {
      "type": "header",
      "content": "Layout"
    },
    {
      "type": "checkbox",
      "id": "hide_mobile",
      "label": "Visibility: hide for mobile"
    },
    {
      "type": "select",
      "id": "layout",
      "label": "Section Layout Mode",
      "default": "boxed",
      "options": [
        { "value": "boxed", "label": "Boxed" },
        { "value": "full", "label": "Wide" },
        { "value": "wide-padding", "label": "Wide padding" }
      ]
    },
    {
      "type": "select",
      "id": "slideshow_height",
      "label": "Slideshow height",
      "default": "auto-height",
      "options": [
        { "value": "auto-height", "label": "Auto" },
        { "value": "full-height", "label": "Full screen" },
        { "value": "manually-height", "label": "Manually" }
      ]
    },
    {
      "type":"range",
      "id":"manually_height",
      "label":"Manually height",
      "min":300,
      "max":800,
      "step": 10,
      "unit":"px",
      "default": 400
    },
    {
      "type": "color",
      "id": "background",
      "label": "Background color",
      "default": "#fff"
    },
    {
      "type": "header",
      "content": "Auto Slides"
    },
    {
      "type": "checkbox",
      "id": "autoplay",
      "label": "Auto rotate between slides",
      "default": true
    },
    {
      "type":"range",
      "id":"autoplay_speed",
      "label":"Change slides every",
      "min":5,
      "max":10,
      "step": 1,
      "unit":"Sec",
      "default":6
    },
    {
      "type": "select",
      "id": "animation",
      "label": "Slideshow Animation",
      "default": "slide",
      "options": [
        { "label": "Fade", "value": "fade" },
        { "label": "Slide", "value": "slide" },
        { "label": "Cube", "value": "cube" },
        { "label": "Coverflow", "value": "coverflow" },
        { "label": "flip", "value": "flip" }
      ]
    },
    {
      "type": "header",
      "content": "Navigator"
    },
    {
      "type": "checkbox",
      "id": "navigator",
      "label": "Show Navigator Next Prev",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "paging",
      "label": "Show Bullet at Bottom",
      "default": false
    },
    {
      "type": "select",
      "id": "paging_position",
      "label":"Bullet position",
      "default": "center-bottom",
      "options": [
        { "value":"left-center", "label": "Left center" },
        { "value":"left-bottom", "label": "Left bottom" },
        { "value":"right-center", "label": "Right center" },
        { "value":"center-bottom", "label": "Center bottom 1" },
        { "value":"center-bottom-2", "label": "Center bottom 2" }
      ]
    },
    {
      "type": "select",
      "id": "paging_style",
      "label":"Bullet style",
      "default": "round",
      "options": [
        { "value":"round", "label": "Round 1" },
        { "value":"round-2", "label": "Round 2" },
        { "value":"line", "label": "Line" }
      ]
    },
    {
      "type": "header",
      "content": "Image gallery & Menu list"
    },
    {
      "type": "select",
      "id": "image_gallery_position",
      "label":"Position",
      "default": "overlay",
      "options": [
        { "value":"overlay", "label": "Image gallery - Overlay 1" },
        { "value":"overlay-2", "label": "Image gallery - Overlay 2" },
        { "value":"overlay-3", "label": "Image gallery - Overlay 3" },
        { "value":"overlay-4", "label": "Image gallery - Overlay 4" },
        { "value":"right", "label": "Image gallery - Right" },
        { "value":"right-2", "label": "Image gallery - Right 2" },
        { "value":"left", "label": "Menu list - Left" },
        { "value":"none", "label": "None" }
      ]
    },
    {
      "type": "checkbox",
      "id": "image_gallery_border",
      "label": "Border content",
      "default": true
    }
