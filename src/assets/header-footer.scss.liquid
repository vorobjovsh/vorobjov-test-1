								}
							}
						}
					}
					.no-title-1 .bp-item {
						padding-top: 0;
					} 
					.bp-item {
						.sb-product-head {
							margin: 0 0 15px;
							.product-name {
								font-size: $product-font-size;
								height: auto !important;
								margin: 0 0 15px;
								> a {
									color: $nav-dropdown-link-color;
									font-size: $product-font-size;
									font-weight: $font-bold;
									display: block;
									padding: 0;
									line-height: 20px;
									&:hover {
										color: $nav-dropdown-link-hover-color;
									}
