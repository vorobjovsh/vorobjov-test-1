@mixin icon-product {
	float: left; 
	width: 36px;
	height: 36px;
	line-height: 34px;
	text-align: center;
	background: $body-background-color;
	color: $body-link-color;
	@include margin-end(2px);
	cursor: pointer;
	&:last-child {
		@include margin-end(0);
	}
	a {
		color: inherit;
		font-size: $body-font-size - 1px;
		height: 100%;
		align-items: center;
		justify-content: center;
		@include flexbox;
		@include transition(none);
