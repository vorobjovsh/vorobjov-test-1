/*=== footer.scss ===*/
@media (max-width: 991px) {
	.footer-col:nth-child(2n+1) { 
		clear: both;
	}
} 
.footer-container {
	background: $footer-background-color;
	.border-top-full {
		border-top: 1px solid rgba($footer-text-color, 0.2);
	}  
	.border-top-container > div > div {
		border-top: 1px solid rgba($footer-text-color, 0.2);
	}
	.border-top-special {
		.footer-inner {
			padding-top: 0 !important;
   			padding-bottom: 0 !important;
			.table-row { 
				border-top: 1px solid rgba($footer-text-color, 0.2);
				border-bottom: 1px solid rgba($footer-text-color, 0.2);
				@media (max-width: 767px) {
					padding: 0 0 15px;
				} 
			}
		}
		.footer-logo-inner {
			@media (min-width: 768px) {
				padding: 25px 0 30px;
			}
			@media (min-width: 992px) {
				padding: 55px 0 60px;
			}
		}
		.footer-check-content > div {
			@media (min-width: 768px) {
				padding: 32px 10px 35px;
				@include padding-start(30px);
				@include border-start(1px solid rgba($footer-text-color, 0.2));
			}  
			@media (min-width: 992px) {
				padding: 65px 10px 70px; 
				@include padding-start(60px);
			} 
		}
	}
	.border-top-special-2 {
		border-bottom: 1px solid rgba($footer-text-color, 0.2);      
		.footer-inner {
			padding-top: 0 !important;
   			padding-bottom: 0 !important;
			.table-row { 
				@media (max-width: 767px) {
					padding: 0 0 15px;
				} 
			}
		}
		.footer-logo-inner {
			@media (min-width: 768px) {
				padding: 25px 0 30px;
			}
			@media (min-width: 992px) {
				padding: 75px 0 77px;
			}
		}  
		.footer-check-content > div {
			@media (min-width: 768px) {
				padding: 32px 10px 35px;
				@include padding-start(30px);
				@include border-start(1px solid rgba($footer-text-color, 0.2));
				&:first-child {
					@include border-start(none);  
				}
			}   
			@media (min-width: 992px) {
				padding: 75px 10px 77px;    
				@include padding-start(30px);  
			} 
			@media (min-width: 1024px) {
				@include padding-start(80px); 
			}
		}
		.newsletter-column .newsletter-title {
			font-size: $size-h1;
			color: $footer-heading-color; 
			font-weight: 500;  
		}
	}
	.widget-social {  
		&.flex {
			float: left;
			padding-bottom: 19px;
			@include padding-start(50px);
			@include padding-end(0);
			@media (max-width: 1440px) {
				@include padding-start(20px);
			}
			@media (max-width: 1300px) {
				float: none;
				padding: 0 0 32px;
				clear: both;
			}
			@media (max-width: 767px) {
				padding-bottom: 15px;
			}
		}
		&.flex-right {
			@include margin-start(auto); 
		} 
		ul li {
			padding: 0 11px;
			@media (max-width: 1199px) {
				padding: 0 5px;
			}
			&:first-child {
				@include padding-start(0);
			}
			&:last-child {
				@include padding-end(0);
			}
			a {
				color: $footer-copyright-color;
				i {
					font-size: $font-small-icon;
				}
				&:hover {
					color: $footer-hover-color;
				}
			}
		}
	}
	.footer-logo {
		margin: 0;
		> h6 {
			@media (max-width: 767px) {
				padding-bottom: 15px;
			} 
		}
		.button {
			clear: both;
			@media (min-width: 768px) {
				margin-bottom: 20px;
			}
			.btn-support {
				margin: 0 0 15px;
			}
		}
		&.fl-border-left {
			@include border-start(1px solid rgba($footer-text-color, 0.2));
			@media (max-width: 767px) {
				@include border-start(none);
			}
			> h6, .footer-logo-inner {
				@include padding-start(78px);
				@media (max-width: 1199px) {
					@include padding-start(30px);
				}
				@media (max-width: 767px) {
					@include padding-start(0);
				} 
			} 
		}
	}
	.flex-logo-layout {
		.footer-logo-inner {
			flex-wrap: wrap;
			align-items: center;
			@include flexbox;
			@media (max-width: 767px) {
				display: block;
			}
			> div {
				padding: 15px 0 0;
				@include padding-end(37px);
				@media (max-width: 991px) {
					padding: 0 0 15px;
					width: 100%;
				}
				&:last-child {
					padding-bottom: 0;
				}
				&.payment-icons {
					padding-top: 15px; 
					@include margin-start(auto);
					@media (max-width: 1199px) {
						@include margin-start(0);
					}
					@media (max-width: 767px) {
						padding-top: 0;
					}
				}
				&.widget-social.flex-right {
					@include padding-end(0);
				}    
			}
			.phone-email {
				@media (min-width: 992px) {
					padding-bottom: 0;
				} 
			}
			.button {
				margin-bottom: 0;
				.btn-support {
					margin-bottom: 0;
				}
			}
			ul {
				margin-top: 0;
			}
		}
	}
	.center-logo-layout {
		text-align: center;
		.footer-block {
			text-align: center;
			display: inline-block;
			.address {
				padding-bottom: 22px;
			}
		}
		.phone-email.flex-email {
			justify-content: center;
		} 
	}
	&.push-elements-right {
		.footer-inner {
			.container {
				@include padding-start(0);
				@include padding-end(0);
			}
			.container-fluid {
				@include padding-start(0);
				@include padding-end(0);
			}
		}
		.footer-logo {
			padding-bottom: 23px;
			@media (max-width: 767px) {
				padding-bottom: 15px;
			}
		}
		.address {
			padding: 0 0 11px;
		}
		.phone-email {
			padding-bottom: 26px;
		}
		.copyright {
			margin-top: 20px;
		}
	}
	/*notice bar*/
	.jewelry-bar {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
		.jewelry-border .jewelry-bar-content {
			border-top: 1px solid $body-border-color;
			border-bottom: 1px solid $body-border-color;
		}
		.jewelry-bar-content {
			margin: 0;
			padding: 8px 0;
			font-size: $body-font-size - 1px;
			text-align: center;
			p {
				margin: 0;
			}
		}
	}
}
.template404 #footer-content {
	display: none;
}
/*=== Footer instagram ===*/
.templateIndex {
	.footer-instagram.heading-relative {
		margin-top: -65px;
		position: relative;
		background: transparent;
		@media (max-width: 767px) {
			margin-top: -40px;
		}
	}
}
.footer-instagram {
	background: $body-background-color;
	.content-inner {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
	}
	.instagram-title {
		text-align: center;
		text-transform: uppercase;
		padding-bottom: 9px;
		a {
			color: $body-link-color;
			font-weight: $font-bold;
			span {
				color: $main-color;
			}
			&:hover {
				color: $body-link-color-hover;
			}
		}
	}
}
/*=== Footer menu ===*/
.footer-widget {
	.footer-inner {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
	}
	.footer-block {
		padding-bottom: 0;
		@media (max-width: 767px) {
			padding-top: 15px !important;
		}
	}
	.fix-col {
		@media (max-width: 767px) {
			max-width: 100%;
			@include flex(0 0 100%);
		}
	}
}
.heading-mobile-only {
	.footer-block {
		h6 {
			@media (min-width: 768px) {
				display: none;
			}
		}
		&:not(.active) ul.f-list {
			padding-top: 0;
		}  
	}
	.fix-menu .footer-block {
		justify-content: center;
	}
}
.fix-menu {
	.footer-block {
		@include flexbox;
		margin: 0;
		align-items: center;
		@media (max-width: 767px) {
			display: block;
		}
		&.textbox .text-1 {
			@media (min-width: 768px) {
				padding: 0;
				@include padding-end(15px);
			}
		}
		h6 {
			padding-bottom: 0;
			@media (min-width: 768px) {
				min-width: 145px;
			}
			.icon {
				top: 50%;
				@include translate(0, -50%);
			}
		}
		.fix-row {
			@include margin-start(0);
			@include margin-end(0);
		}
		ul.f-list {
			display: block;
			li {
				display: inline-block;
				padding: 0 15px;
				@media (max-width: 767px) {
					padding: 0 5px;
				}
			}
		}
	}
}
.heading-flex {
	.footer-block {
		overflow: hidden;
		h6 {
			padding-bottom: 0;
			@media (min-width: 992px) {
				width: 185px;
				text-align: start;
				float: left;
			}
		}
		> div,
		> ul {
			@media (min-width: 992px) {
				float: left;
				width: calc(100% - 185px);
			}
		}
		.logo-footer {
			width: auto;
		}
		.widget-social {
			width: auto;
		}
		&.payment-icons {
			ul {
				@media (max-width: 767px) {
					padding-top: 15px;
				}
			}
			li:first-child {
				@include padding-start(0);
			}
		}
	}
	.content-inner {
		overflow: hidden;
		.newsletter-heading-block {
			@media (min-width: 992px) {
				width: 185px;
				float: left; 
				text-align: start;
			}
		}
		.footer-newsletter {
			@media (min-width: 992px) {
				float: left; 
				width: calc(100% - 185px);
			}
		}
		.newsletter-inner form .action-form input {
			@include margin-end(0);
		} 
	}
}
.footer-block {
	h6 {
		color: $footer-heading-color;
		font-weight: $font-bold;
		position: relative;
		padding-bottom: 31px;
		@media (max-width: 767px) {
			cursor: pointer;
			padding-bottom: 0;
		}
	}
