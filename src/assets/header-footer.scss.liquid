.horizontal-menu {
	background: none;
	position: static;  
	.menu-list {
		width: 100%;
		position: static;
		margin: 0 auto;
		border: none;
		@include border-radius-reset;
		> li {
			float: left; 
			margin: 0;
			padding: 2px 0;
			@include padding-start(34px);
			@include transition(all 0.35s ease 0s);
			&:first-child {
				@include padding-start(0);
			} 
			> .dropdown-inner > a,
			> a {
				font-size: $nav-font-size;
				margin: 0;
				display: inline-block;
				text-decoration: none;
				line-height: 25px;
				padding: 9px 0;
				color: $nav-link-color;
				position: relative;
				&:after {
					content: "";
					@include ab(absolute, block, n, 0, 6px, n);
					width: 0;
					background: $nav-link-hover-color;
					height: 2px;
					@include transition(width 0.3s ease-in-out);
				}
				&:hover {
					color: $nav-link-hover-color;
				}
			}
			> .dropdown-inner > span.expand,
			> span.expand {
				color: $nav-link-color;
			}
			/*have submenu*/
			&.dropdown {
				position: relative;    
				.dropdown-menu li {
					position: relative;  
					.dropdown-link {
						@include margin-end(30px);  
					}
					span.expand {
						height: 32px;
						width: 30px;
						text-align: center;
						line-height: 32px;
						@include ab(absolute, block, 8px, n, n, 17px);
						font-size: $nav-sub-font-size;
						&:before {
							content: '\e876';
							font-family: $font-family-icon;
						}
					}
					&.flyout-left {
						.dropdown-inner {
							> a.dropdown-link {
								@include margin-end(0);
							}
							> span.expand {
								@include ab(n, n, n, 0px, n, auto);
								&:before {
									content: '\e875';
								}
							}
						}
						> a.dropdown-link {
							@include margin-end(0);
						}
						> span.expand {
							@include ab(n, n, n, 0, n, auto);
							&:before {
								content: '\e825';
							}
						}
						> .dropdown-menu {
							@include ab(n, n, n, auto, n, 100%);
							@include border-end(1px solid $body-border-color);
							@include border-start(none);   
						}
					}
				}
				> .dropdown-menu {
					@include ab(n, n, 100%, 0, n, n);
					background: $nav-dropdown-background;
					font-size: $nav-sub-font-size;
					> li {
						> a {
							color: $nav-dropdown-link-color;
							white-space: normal;
							span {
								color: $nav-dropdown-link-color;
								@include transition(all 0.35s ease 0s);
							}
						}
						.dropdown-menu {
							background: $nav-dropdown-background;
							@include border-start(1px solid $body-border-color);  
							> li {
								span.expand {
									color: $nav-dropdown-link-color;
								}
								> a {
									color: $nav-dropdown-link-color;
									white-space: normal;
									span {
										color: $nav-dropdown-link-color;
									}
									&:hover span {
										color: $nav-dropdown-link-hover-color;
									}
								}
								&:hover {
									> span.expand {
										color: $nav-dropdown-link-hover-color;
									}
									> a span {
										color: $nav-dropdown-link-hover-color;
									}
								}
								&:last-child > a {
									border-bottom: none;
								}
							}
						}
						&:hover {
							> span.expand {
								color: $nav-dropdown-link-hover-color;
							}
							> a span {
								color: $nav-dropdown-link-hover-color;
							}
						}
						&:last-child > a {
							border-bottom: none;
						}
					}
					> li.dropdown-submenu > .dropdown-menu {
						background: $nav-dropdown-background;
						margin-top: -11px;   
					}
					.product-button {
						display: none;
					}
				}
				.dropdown-menu-sub {
					background: transparent;
					border: none;
					box-shadow: none;
					position: static;
					width: 100%;
					min-width: auto;
					padding-top: 0;
					@include border-radius-reset;
					> li {
						padding: 5px 0 0;  
						> a {
							color: $nav-dropdown-link-color;
							white-space: inherit;
							border: none;
							span {
								color: $nav-dropdown-link-color;
							}
							&:hover span {
								color: $body-background-color;
							}
						}
					}
					.product-wrapper {
						margin: 0;
						.product-vendor {
							display: none;
						}
						.product-add-cart {
							display: none;
						}
						.product-name {
							color: $nav-dropdown-link-color;
							&:hover {
								color: $nav-dropdown-link-hover-color;
							}
						}
					}
					&.menu-proudct-carousel {
						li:hover a {
							text-decoration: none !important;
						}
						.owl-nav button {
							@media (min-width: 1025px) {
								@include opacity(0);
							}
							&.owl-prev {
								@include ab(n, n, n, 0, n, n);
							}
							&.owl-next {
								@include ab(n, n, n, n, n, 0);
							}
						}
