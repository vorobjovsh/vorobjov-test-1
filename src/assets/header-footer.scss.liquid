					}   
					&.logo-list { 
						img {
							display: block;
							margin: 10px auto 0;
						}
						.text {  
							display: block;
							text-align: center;
							font-size: 13px;
							font-weight: 500;
							text-transform: uppercase;
							padding-top: 15px; 
							padding-bottom: 15px;   
						} 
