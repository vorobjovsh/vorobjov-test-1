								} 
								.textbox {
									width: 41.666667%; 
									order: 1;
									@include padding-end(27px);
								}
							}
							&.image-textbox-overlay {
								position: relative;
								.textbox {
									text-align: center; 
									@include ab(absolute, block, 50%, 50%, n, n);
									@include translate(-50%, -50%);
									.text {
										margin-bottom: 20px;
										font-size: 50px; 
										line-height: 1; 
									}
								}
							}
							> a {
								padding: 0;
								display: block;
								span { 
									display: block;
									font-weight: 500;
									text-transform: uppercase;
									@include transition(all 0.35s ease 0s);
								}  
								&:after {
									content: none;   
								}
								&:hover span {
									color: $nav-link-hover-color;
								}  
							}
							&:hover {
								background: none;
							}
							&:last-child {
								margin-bottom: 0;
							}
