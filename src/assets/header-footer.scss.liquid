			.custom-html {
				padding: 10px 20px;
				color: $nav-dropdown-link-color;
			} 
			.dropdown-menu {
				background: inherit;    
				> .row {
					margin: 0;
					position: static;
				}
				.mega-sub-link {
					overflow: hidden;
