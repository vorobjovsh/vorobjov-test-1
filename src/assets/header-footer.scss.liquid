						.bp-content-inner {
							padding: 5px 0 1px;
						}
						&:hover {
							background: none;
							a {
								text-decoration: none !important;
							}
						}
					}
					&.no-title-0 {
						> a,
						> span.expand {
							display: none !important;
						}
					}
					&.no-title-2 {
						> a,
						> span.expand {
							display: none !important;
						}
					}
					&.banners {
						margin: 0;
						img {
							@include opacity(0);
							visibility: hidden;
						}
						&.no-title-1 {
							> a,
							> span.expand {
