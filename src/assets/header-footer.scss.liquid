					list-style: none;
					padding: 5px 0;
					i {
						@include margin-end(5px);
					}  
					a {
						display: block;   
						height: auto;
						line-height: normal;  
						font-size: $body-font-size; 
						padding: 0;    
						&:hover {
							color: $body-link-color-hover;
						} 
					}     
				}
			} 
		}
        .currency-position { 
            position: relative;
            display: block;
			margin-bottom: 50px;
            select {
                width: 100%;
                background-color: transparent;
                border: 1px solid $body-border-color;      
                height: 37px;
                padding: 0px 10px;
                display: block;
                color: $body-general-color;  
                line-height: 36px; 
                background-image: none !important; 
                position: relative;  
                z-index: 1;  
            }    
            .select-group {
				position: relative;
				i {
					@include ab(absolute, block, 50%, n, n, 10px);
					@include translate(0, -50%); 
					&:before { 
						font-size: $font-small-icon; 
					}   
				}
			}
			.currency_wrapper { 
				padding: 0px 10px;
				position: relative;
				height: 37px;
				line-height: 36px;
				color: $body-general-color;
				border: 1px solid $body-border-color;
				align-items: center; 
				@include flexbox;
				.expand {
					font-size: 15px;
					width: 20px;
					line-height: 28px;
					height: 100%;   
					text-align: center; 
					color: inherit;   
					@include margin-start(auto);
					i {
						display: block; 
						width: 100%;
						height: 100%; 
						font-size: $font-small-icon;
						line-height: 35px;   
					}   
				}
			}
			.currency-flag {
				display: none;
			} 
			.type-code .c-name {
				display: none;  
			}
			.type-symbol .c-code {
				display: none;
			}
			a:hover { 
				text-decoration: none; 
			}
			ul.currencies-dropdown {
				@include opacity(0);
				visibility: hidden;
				&.opened {
					@include opacity(1);
					visibility: visible;
				}   
			} 
        }
        .translate-position-mobile {
            position: relative;
            display: block;  
            .weglot-container { 
                display: block;    
            }     
            .country-selector.closed ul {
				@include dropdown-hidden; 
				@include translate(0, 10px);    
			}       
            .wgcurrent {
                color: $body-general-color;  
                padding: 0px 10px;
                position: relative;
                height: 37px;
                line-height: 36px;
                cursor: pointer; 
                border: 1px solid $body-border-color; 
				z-index: 9; 
				align-items: center;  
                @include flexbox;
                a {
                    height: auto;
                    padding: 0; 
                    color: $body-general-color; 
                    font-size: $body-font-size;    
                } 
                &:after {
                    content: "\e812" !important;
                    font-family: $font-family-icon;
                    font-size: 22px;   
                    background-image: none;
                    width: 20px;  
                    height: 37px;  
                    text-align: end;
                    line-height: 37px;       
                    position: static;
                    @include margin-start(auto);
                    @include transform(none);  
                }            
            }    
            ul {  
                @include dropdown-visible;
                @include translate(0, 0);     
			}
