					}
					&.sub-link-expire {  
						> a, .wrapper-countdown {
							display: inline-block;
							vertical-align: top;
						} 
						> a {
							font-size: $size-h1 + 10px; 
							max-width: 210px;
							line-height: 1.1;
							@include margin-end(30px);
						}  
						.is-countdown {  
							margin-bottom: 0;  
							.countdown-section {
								background: transparent;
								@include border-end(none);
								.countdown-period {
									color: $nav-dropdown-link-color; 
								} 
							}
						} 
						.menu-proudct-row {
							margin-top: 25px; 
						}
					}     
				}
				.product-title a {
					color: $nav-dropdown-link-color !important;
					&:hover {
						color: $nav-dropdown-link-hover-color !important;
					}
				}
				.content-text {  
					display: block;
					text-align: center;
					h3 {
						font-size: 25px;
						font-weight: 400;
						color: $nav-dropdown-link-hover-color; 
						display: block;     
					}
					h2 {
						font-size: 50px;
						display: block;
						margin-bottom: 16px;
						color: $nav-dropdown-link-color;
					}
				} 
				&.dropdown-menu-tabs {
					.row {
						margin: 0;
					}
					.row-1 {
						margin: 0 auto;
					}
					.row-product > li {
						padding: 0 10px;
						margin: 0; 
					} 
				}
