		}
	}
	.text-position-right-center .item {
		align-items: center;
	}
	.border-right-item {
		.row-1 > div:not(.line) {
			@include border-start(1px solid rgba($footer-text-color, 0.2));
			@media (max-width: 767px) {
				@include border-start(none);
			}
			&:first-child {
				@include border-start(none);
				.item {
					@include padding-start(0);
				}
			}
		}
		.item {
			@include padding-start(40px);
			@media (max-width: 1199px) {
				@include padding-start(15px);
			}
			@media (max-width: 767px) {
				@include padding-start(0);
			}
		}
	}
	.border-radius-round {
		.service-bar-container {
			border: 1px solid rgba($footer-text-color, 0.2);
			padding: 12px 10px;
			@include border-radius-50;
			@media (max-width: 767px) {
				padding: 5px 10px;
			}
			.row-1 {
				.flex-item {
					@media (min-width: 768px) {
						max-width: 100%;
						@include flex(1);
					}
				}
				> div:first-child .item {
					@media (max-width: 767px) {
						border-top: none;
					}
				}
				.item {
					@media (max-width: 767px) {
						padding: 5px 10px !important;
						border-top: 1px solid rgba($footer-text-color, 0.2);
					}
				}
			}
		}
	}
	.border-radius-square {
		.service-bar-container {
			border: 1px solid rgba($footer-text-color, 0.2);
			.row-1 {
				.flex-item {
					@media (min-width: 768px) {
						max-width: 100%;
						@include flex(1);
					}
				}
				> div {
					@media (min-width: 768px) {
						padding: 0 10px;
					}
					&:first-child .item {
						@include padding-start(10px);
						@media (max-width: 767px) {
							border-top: none;
						}
					}
					&:last-child .item {
						@include padding-end(10px);
					}
				}
				.item {
					padding: 7px 10px;
					@media (max-width: 767px) {
						padding: 5px 10px !important;
						border-top: 1px solid rgba($footer-text-color, 0.2);
					}
				}
			}
		}
	}
	.line {
		border-bottom: 1px solid rgba($footer-text-color, 0.2);
		@include flex(1);
		@media (max-width: 991px) {
			display: none;
		}
		&:last-child {
			display: none;
		}
	}
	.text-position-below .item {
		display: block;
		text-align: center;
		@include padding-start(0);
		.icon {
			margin: 0 auto;
			@include padding-end(0);
		}
		.text {
			padding-top: 10px;
		}
	}
	.item {
		position: relative; 
		@include flexbox;
		@media (max-width: 767px) {
			padding-bottom: 10px;
		}
		.icon {
			color: $main-color;
			@include padding-end(20px);
			@include flex(none);
			i:before {
				font-size: 45px;
				@media (max-width: 767px) {
					font-size: 30px;  
				}
			}
		}
		.text {
			.heading {
				line-height: 1.1;
				font-weight: $font-bold;
				span {
					text-decoration: underline;
				}
			}
			p {
				padding-top: 5px;
				font-size: $body-font-size;
				color: $footer-copyright-color;
				margin: 0;
			}
		}
	}
	.style-1 .row-1 > div {
		@media (min-width: 768px) {
			max-width: 33.33333%;
			@include flex(0 0 33.33333%);
		}
	}   
	.style-2,
	.style-4 {
		.row-1 {
			align-items: center;
			> div:not(.line) {
				max-width: 100%;
				width: auto;
				margin: 0 40px;
				@include border-start(none);
				@include flex(0 0 auto);
				@media (max-width: 1199px) {
					margin: 0 10px;
				}
				@media (max-width: 991px) {
					margin: 0;
				}
				@media (max-width: 767px) {
					@include flex(0 0 100%);
				}
				&:first-child {
					@include margin-start(0);
				}
				&:nth-last-child(-n+2) {
					@include margin-end(0);
				}
				.item {
					@include padding-start(0);
				}
			}
		}
	}
	.style-2 {
		> .row-1 > div:nth-last-child(2) {
			.item {
				@media (max-width: 767px) {
					padding-bottom: 0;
				}
			}
		}
	}
	.style-3 .item .icon {
		@include padding-end(18px);
	}
	.style-4 .item .icon {
		@include padding-end(18px);
		@media (max-width: 1199px) {
			@include padding-end(10px);
		}
	} 
	.style-5 {
		.row-1 {
			justify-content: center;
			.flex-item {
				padding: 0 10px;
				@media (max-width: 767px) {
					width: 100%;
				}
				&:first-child .item {
					@include padding-start(0);
				}
				&:last-child .item {
					@include padding-end(0);
				}
			}
		}
		.item {
			justify-content: center;
			@include padding-start(30px);
			@include padding-end(30px);
			@media (max-width: 1199px) {
				@include padding-start(10px);
				@include padding-end(10px);
			}
			@media (max-width: 767px) {
				justify-content: initial;
				@include padding-start(0);
				@include padding-end(0);
				.icon {
					min-width: auto;
				}
			}
			.text .heading {
				font-weight: 400;
			}
		}
	}
	.style-6 {
		.row-1 .flex-item {
			padding: 0 10px;
			@media (max-width: 767px) {
				width: 100%;
			}
			&:first-child .item {
				@include padding-start(0);
			}
			&:last-child .item {
				@include padding-end(0);
			}
		} 
		.item {
			@include padding-start(30px);
			@include padding-end(30px);
			@media (max-width: 1199px) {
				@include padding-start(10px);
				@include padding-end(10px);
			}
			@media (max-width: 767px) {
				@include padding-start(0);
				@include padding-end(0);
				.icon {
					min-width: auto;
				}
			}
			.text .heading {
				font-weight: 400;
			}
		}
	}
	.style-7 {
		.row-1 > div:not(.payment-icons){
			@media (min-width: 992px) {
				max-width: fit-content; 
				@include flex(0 0 auto); 
				@include padding-end(48px); 
			}
		} 
		.item {
			align-items: center; 
		}
		.payment-icons {
			padding: 0 10px; 
			@include margin-start(auto);
			@media (max-width: 1199px) {
				padding-top: 15px;  
				width: 100%; 
				@include margin-start(0);
			}
		}
	}
}
/*=== support center ===*/
.support-center {
	h5 {
		color: $footer-heading-color;
	}
	.time {
		color: $footer-text-color;
		font-size: $body-font-size - 2px;
		margin-top: 9px;
	}
}
.btn.btn-support { 
	font-size: 11px;
	padding: 1px 22px;
	margin-top: 15px;
	display: inline-block;
	background: $button-btn2;
	color: $button-btn2-txt;
	border: 1px solid $button-btn2-border;
	&:hover {
		background: $button-hover-btn2;
		color: $button-hover-btn2-txt;
		border: 1px solid $button-hover-btn2-border;
	}
}
/*=== Footer logo list ===*/
.footer-container {
	.logo-list-content {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
	}
	.logo-list-carousel {
		.item {
			width: 10%;
			display: inline-block;
		}
		.owl-stage {
			margin: 0 auto;
		}
		&.owl-loaded .item {
			width: 100%;
			display: block;
			img {
				width: auto;
			}
		}
	}
}

/*=== Footer working time ===*/
.working-time {
	.phone {
		font-size: $size-h1 + 6px;
		font-weight: 500;
		@media (max-width: 991px) {
			font-size: $size-h1 - 4px; 
		}
		span {
			position: relative;  
			top: -12px;
			font-size: $size-h1  - 10px;
			@media (max-width: 991px) {
				font-size: 15px;  
				top: -7px;  
			}
		}
	}
	.working {
		font-size: $body-font-size - 2px;
		color: $footer-text-color; 
		padding-top: 9px;
	}
	.email-text {
		margin-top: 26px;
		padding-top: 28px; 
		border-top: 1px solid rgba($footer-text-color, 0.2);  
		font-weight: 500;
		font-size: $body-font-size + 1px; 
		a {
			color: $main-color;  
		}
	}
	.widget-social-icons {  
		padding-top: 20px;    
		li {  
			@include padding-end(17px); 
			a {
				color: $footer-link-color;
				&:hover {
					color: $footer-hover-color;    
				}
			}   
		} 
		i:before { 
			font-size: $font-small-icon;
		}
	}
}

/*=== Footer copyright ===*/
.footer-bot {
	background: $footer-copyright-bg;
	.footer-bot-container {
		@media (max-width: 767px) {
			padding-top: 15px !important;
			padding-bottom: 15px !important;
		}
	}
	.footer-bot-container {
		@media (max-width: 767px) {
			> div {
				padding-top: 15px;
				> div {
					padding-top: 15px;
					&:first-child {
						padding-top: 0;
					}
				}
				&:first-child {
					padding-top: 0;
				}
			}
		}
		&.text-center {
			> div {
				padding-top: 18px;
				> div {
					padding-top: 18px;
					&:first-child {
						padding-top: 0;
					}
				}
				&:first-child {
					padding-top: 0;
				}
			}
		}
	}
	.text-left,
	.text-right {
		.default-copyright {
			flex-wrap: wrap;
			align-items: center; 
			@include flexbox;
			@media (max-width: 991px) {
				display: block;
			}
			> div:nth-child(2) {
				@include margin-start(auto);
			}
		}
	}
	&.fb-style-1 {
		.social-payments {
			flex-wrap: wrap; 
			@include flexbox;
			@media (max-width: 767px) {
				display: block;
			}
			> div:nth-child(2) {
				@include margin-start(auto);
				@media (min-width: 768px) {
					padding-top: 0;
				}
			}
		}
	}
	&.fb-style-2 {
		.payment-icons {
			padding-top: 27px;
			@media (max-width: 767px) {
				padding-top: 15px;
			}
		}
		.copyright {
			padding-top: 14px;
		}
	}
	&.fb-style-3 .footer-bot-container {
		flex-wrap: wrap;
		align-items: center; 
		@include flexbox;
		@media (max-width: 767px) {
			display: block;
		}
		.copyright {
			padding-top: 0;
		}
		> div:nth-child(2) {
			@include margin-start(auto);
			@media (min-width: 768px) {
				padding-top: 0;
			}
		}
	} 
	&.fb-style-4 {
		.footer-bot-container {
			flex-wrap: wrap;
			align-items: center; 
			@include flexbox;
			@media (max-width: 767px) {
				display: block;
			}
			.widget-social {
				@include margin-start(auto);
			}
			.copyright {
				padding-top: 0;
			}
			.default-copyright {
				@include margin-start(auto);
			}
		}
		.support-center-content {
			flex-wrap: wrap;
			align-items: center; 
			@include flexbox;
			@media (max-width: 991px) {
				display: block;
			}
			.button {
				@include margin-start(74px);
				@media (max-width: 991px) {
					@include margin-start(0);
				}
			}
		}
	}
	.payment-icons {
		@media (max-width: 767px) {
			padding-top: 15px;
		}
		ul li {
			padding: 0 2px;
			color: $footer-copyright-color;
			&:first-child {
				@include padding-start(0);
			}
			&:last-child {
				@include padding-end(0);
			}
		}
		i {
			font-size: $font-icon; 
		}
	}
	.copyright {
		padding-top: 32px;
		color: $footer-copyright-color;
		font-size: $body-font-size - 2px;
		@media (max-width: 767px) {
			padding-top: 15px;
		}
		p {
			margin: 0;
		}
	}
	.default-copyright .info {
		@media (max-width: 1200px) {
			padding-top: 10px;
		}
	}
	.info a {
		color: $main-color;
		@include padding-start(5px);
	}
	.copyright-menu {
		ul {
			overflow: hidden;
			display: inline-block;
			li {
				padding: 0 10px;
				@media (max-width: 1199px) {
					padding: 0 5px;
				}
				&:first-child {
					@include padding-start(0);
				}
				&:last-child {
					@include padding-end(0);
				}
				a {
					font-size: $body-font-size - 2px;
					font-weight: 400;
					&:hover {
						color: $footer-hover-color;
					}  
				}
			}
		}
		&.bold-text ul li a {
			font-weight: 600;
		}
	}
}
/* Section image gallery */
.wide-padding {
	#home-banner-section-image-gallery.home-banner.layout-full {
		@include padding-start(0);
		@include padding-end(0);
	}
}
#home-banner-section-image-gallery {
	.banner-item .btn {
		@media (max-width: 767px) {
			display: none;
		}
	} 
}