				width: calc(100vw - 30px - 17px - 260px);     
				@media (min-width: 1200px) {
					width: calc(100vw - 140px - 17px - 260px);
				}    
			}
			&.width-75 {
				width: calc((100vw - 30px - 17px - 260px) * 3 / 4);   
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) * 3 / 4);
				}
			}
			&.width-50 {
				width: calc((100vw - 30px - 17px - 260px) / 2);
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) / 2); 
				}
			}
			&.width-25 {
				width: calc((100vw - 30px - 17px - 260px) / 4); 
				@media (min-width: 1200px) {
					width: calc((100vw - 140px - 17px - 260px) / 4); 
				}
			}
		}
	}
