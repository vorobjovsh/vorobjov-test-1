		.product-button {
			display: none !important;
		}
		span.expand {
			z-index: 5;
			height: 46px;
			line-height: 46px;
			text-align: end;
			padding: 0 15px;
			font-size: 14px;   
			cursor: pointer;
			color: $nav-dropdown-link-color;
			&:before {
				@include next-icon;
				font-family: $font-family-icon;
				font-size: $font-small-icon - 7px;
				display: block;
				width: 16px;     
				line-height: 46px;
				@include transition(all 0.35s ease 0s);
			}
		}
		.mega-menu {
			.mega-image-bottom {
				padding: 0;
				> a {
					padding: 15px;
				}
			}
			.custom-html .content-text {
				display: block;
				text-align: center;
				h3 {
					font-size: 15px; 
					font-weight: 400;
					color: $nav-dropdown-link-hover-color; 
					display: block;     
				}
				h2 {
					font-size: 25px;
					display: block;
					margin-bottom: 16px;
					color: $nav-dropdown-link-color;
				}
			}
			.is-countdown { 
				padding: 10px 20px;
				.countdown-section {
					text-align: center;
					padding: 0;
					@include border-end(none); 
					@include margin-end(7px); 
					&:last-child {
						@include margin-end(0);  
