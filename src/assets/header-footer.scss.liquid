        @media (max-width: 991px) {
            width: 50%;
            margin: 0 calc((100% - 50%) /2); 
        }   
        @media (max-width: 767px) {
            width: 90%;
            margin: 0 calc((100% - 90%) /2);
        }   
    }  
    .modal-content {
        border: none;
        @include border-radius-reset;  
    }  
    .language-close i:before {
        @include close-btn;     
    }          
    .modal-header { 
        padding: 0;
        border: none;
        @include border-radius-reset;
    }  
    .modal-body {
        padding: 25px;
        h4 {
            padding: 0 0 10px;
            text-transform: uppercase;  
		}
