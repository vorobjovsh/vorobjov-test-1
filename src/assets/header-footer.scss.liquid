					.text {  
						display: block;
						text-align: center;
						font-size: $nav-sub-font-size - 1px;
						padding-top: 25px;  
					} 
					.row-col {
						overflow-x: auto;
						overflow-y: hidden; 
						flex-wrap: nowrap; 
						> div > a {
							display: block;
							margin: 0 auto; 
						}   
					}  
				}     
				.logolist-border-all {
					margin: 0;
					> div {
						border-top: 1px solid $body-border-color;
						@include border-start(1px solid $body-border-color); 
						&.col {
							border-top: none; 
						}
						&.col-lg-2:nth-child(-n+6) {
							border-top: none;
						}
						&.col-lg-3:nth-child(-n+4) {
							border-top: none;  
						}
						&.col-lg-4:nth-child(-n+4) {
							border-top: none;  
						}
						&.col-lg-6:nth-child(-n+2) {
							border-top: none;  
						}
						&.col-lg-5:nth-child(-n+2) {
							border-top: none;  
						} 
						&.col-lg-7, &.col-lg-8, &.col-lg-9, &.col-lg-10, &.col-lg-11, &.col-lg-12 { 
