			.mm-border {
				border: 2px solid $main-color; 
			}     
			.dropdown-menu-box {
				> div {
					margin: 0;   
				}
				&.mm-justify-content > div {
					justify-content: center; 
				} 
				.row-border-top > div { 
					position: relative;     
					&:before {  
						content: "";
						@include ab(absolute, block, 0, 0, n, 0);
						height: 1px;
						background: $body-border-color;
					} 
				}      
			}
			.mega-col img {
				@include box-shadow(0 0 0 $white);      
				&:hover {
					@include box-shadow(0 0 0 $white);
				}
			}
			.mm-tab-col-title {
				padding: 0;
				.tab-title {
					margin: 0;
					padding: 0;
					height: 100%;
					> li {
						line-height: 20px;
						cursor: pointer;
						color: $nav-dropdown-link-color;
						font-size: $nav-sub-font-size + 2px;
						font-family: $font-heading;
						font-weight: $font-bold;
						border-bottom: 1px solid $body-border-color;
						padding: 14px 20px 16px; 
						@include padding-start(15px);
						@include transition(all 0.35s ease 0s);
						@media (min-width: 1200px) {
							@include padding-start(70px);
						}
						> img, > svg, > span {
							display: inline-block;
							vertical-align: middle;
						}
						> img {
							@include margin-end(15px);
						} 
						> svg {   
							width: 30px;
							height: 30px; 
							@include margin-end(17px);
						}
						&.active {
							background: $white;
						}
						&:empty {
							display: none;
						}
					}
				}
			}
			.mm-tab-col-content {
				position: relative;
				height: 100%;
				padding: 0;
				.tab-content-inner {
					@include ab(absolute, block, 0, 0, n, 0);
					@include opacity(0);
					visibility: hidden;
					z-index: -99;
					padding: 34px 20px 45px;  
					@include padding-end(15px);
					@include transition(opacity 0.35s); 
					@media (min-width: 1200px) { 
						@include padding-end(70px);
					} 
					.m-list {
						margin: 0;
						padding: 0;
						li a {
							line-height: 20px;
						}
					}
				}
			}
			&.mega-is-hover {
				> .dropdown-menu-tabs {
					.mm-tab-col-content .tab-content-inner.active {
						@include opacity(1);
						visibility: visible;
						z-index: 1;
						position: static;
					} 
				}
				> .dropdown-menu .mega-sub-link.banners img {
					@include opacity(1);
					visibility: visible;
				}
			}
		}
	}  
}
#menu-hover-background {     //black shadow when hover menu
	@include ab(fixed, n, 0, 0, 0, 0);
	background: rgba(0, 0, 0, 0.5);
	height: 100vh;
	z-index: 999;  
	width: 100%;    
	@include dropdown-hidden;
	@include transition(all 0.2s ease-in-out);
	&.active {
		@include dropdown-visible;
	}
}
.header-container,  
.header-department #shopify-section-announcement-bar, 
.header-department #shopify-section-notice-bar {
	background: $bg-header-color;    
	@media (min-width: 992px) {
		position: relative; 
		z-index: 9999;       
	}     	
}  
.sidemenu-holder {     // horizontal & vertical
	.dropdown-menu {
		padding: 11px 0 25px;      
		> li {
			padding: 14px 35px 0; 
			.dropdown-inner > a,
			> a {
				line-height: 20px;
				position: relative;
				padding: 0;
				display: inline-block;
				color: $nav-dropdown-link-color;
				font-size: $nav-sub-font-size;
				text-decoration: none;
				&:after {
					content: "";
					@include ab(absolute, block, n, 0, 1px, n);
					width: 0;
					background: $nav-dropdown-link-hover-color;
					height: 1px;
					@include transition(width 0.3s ease-in-out);
				}
				&:hover {
					color: $nav-dropdown-link-hover-color;
				}
			}
			> .dropdown-inner > span.expand {
				color: $nav-dropdown-link-color;
			}
			&:hover {
				> .dropdown-inner > a, 
				> a {
					&:after {
						@media (min-width: 1025px) {
							width: 100%;
						}
					}
				}
				> .dropdown-inner {
					> a {
						color: $nav-dropdown-link-hover-color;
						text-decoration: none;
					}
					> span.expand {
						color: $nav-dropdown-link-hover-color;
					}
				}
			}
		}
	}
	.navbar {
		position: static;
		border: none;
		margin: 0;
		padding: 0;
		@include box-shadow(0 0 0 transparent);
		@include border-radius-reset;
		.navbar-collapse {
			position: static;
			@include padding-start(0);
			@include padding-end(0);
			> ul > li {
				> .dropdown-inner > a, 
				> a {
					> img, > svg, > span {
						display: inline-block;
						vertical-align: middle;
						+ span +.labels-wrapper {
							@include ab(n, n, n, 39px, n, n);
						} 
					} 
					> img {
						max-width: 25px;
						visibility: inherit;
						@include margin-end(9px);
						@media (max-width: 1200px) {
							@include margin-end(4px);
						}
					} 
					> svg {   
						width: 25px;
						height: 25px; 
						@include margin-end(9px); 
					}   
				}
				> .dropdown-menu .mega-sub-link { 
					&.size-1:not(:last-child) { 
						padding-bottom: 35px;
					} 
					&.horizontal-mode {
						float: left; 
						padding-bottom: 18px;  
						&:not(.size-1) {
							@include padding-start(15px);
							&:first-child {
								@include padding-start(0);
							}  
						}
						> a {
							min-height: 30px;  
						}
						&.size-1 {
							width: 100%;
						}
						&.size-2 {
							width: 50%;
							&:nth-child(2n+1) { 
								clear: both;
								@include padding-start(0); 
							}
						}
						&.size-3 {
							width: 33.33333%;
							&:nth-child(3n+1) { 
								clear: both;
								@include padding-start(0);
							}   
						}
						&.size-4 {
							width: 25%; 
							&:nth-child(4n+1) { 
								clear: both;
								@include padding-start(0);
							}
						}
						&.size-5 {
							width: 20%; 
							&:nth-child(5n+1) { 
								clear: both;
								@include padding-start(0);
							}
						}
						&.size-6 {
							width: 16.66667%; 
							&:nth-child(6n+1) {  
								clear: both;
								@include padding-start(0);
							} 
						}
					}
					&.big-size > a {
						font-size: $size-h1 + 10px;
						font-weight: $font-bold;  
						margin-bottom: 32px;
						line-height: 1.1;
					}
					> a {
						font-size: $nav-header-sub-size; 
						position: relative;
						display: block;
						padding: 0 0 6px;
						margin: 0 0 8px;  
						line-height: 24px;
						color: $heading-mm-color;    
						&:hover {
							color: $nav-dropdown-link-hover-color;
						}
						&.mega-image {
							padding: 0;
							margin: 15px auto 18px;
							border: none;
							position: relative;
						}
					} 
					&:not(.dropdown) > a {
						margin-bottom: 6px;  
					}
					.menu-proudct-row {
						width: 100%;
						flex-wrap: wrap; 
						@include flexbox;
					} 
					.is-countdown {
						margin-bottom: 35px;  
						.countdown-row {
							width: fit-content; 
						}
						.countdown-section {
							text-align: center;
							padding: 0;
							@include border-end(none);
							@include margin-end(7px);
						} 
						.countdown-amount {
							background: #e5e5e5;
							color: $body-general-color;
							padding: 10px;  
							text-align: center; 
							margin-bottom: 5px;
							min-width: 40px; 
							min-height: 40px;  
							@media (min-width: 1440px) {
								font-size: 23px;
								min-width: 60px;
								min-height: 60px; 
								line-height: 39px; 
							}
						}
					}
				}
			} 
		}
	}
}
.labels-wrapper {
	@include ab(absolute, block, -7px, 0, n, n);
	z-index: 2;
	text-transform: uppercase;
	.menu-label {
		display: block;
		font-size: 10px;
		font-weight: 400;
		padding: 0 3px;
		line-height: 13px;
