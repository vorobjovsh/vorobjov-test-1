				.col-border {
					@include border-end(1px solid $body-border-color);    
				} 
				.mega-sub-link {
					> span.expand {
						display: none;
					}
					.dropdown-menu {
						margin: 0;
					}  
					.dropdown-menu-sub {
						padding: 0;
						&:before {
							content: none;
						}
						li {
							border: none;
							color: $nav-dropdown-link-color;
							&.bp-item-2 {
								padding-top: 15px;
							}
							&:hover {
								background: none;
								> a {
									color: $nav-dropdown-link-hover-color;
									&:after {
										width: 100%;
