							font-size: $size-h1 + 10px;  
							font-weight: 500;
						}  
						.btn { 
							margin-top: 28px; 
							text-transform: capitalize;
						}  
					} 
				}  
				.p-style-3 .product-wrapper .product-content {
					@include ab(absolute, block, 26px, 30px, n, n); 
				} 
				.p-style-4 .product-wrapper {
					padding-top: 5px;
					align-items: center;
					@include flexbox;
					.product-head {
						width: 45%;
						@include padding-end(30px);
					}
				}  
				&:not(.mega-menu).mega-is-hover > .dropdown-menu {
					@include dropdown-visible;  
					z-index: 99999; 
				}       
				&.mega-menu {
					position: static;
					> .dropdown-menu {
						&.position-center {
							@include ab(n, n, n, 50%, n, n);   
							@include translate(-50%, 0);   
						}  
						&.position-right {
							@include ab(n, n, n, auto, n, n);
						} 
						.dropdown-menu {
							font-size: $nav-sub-font-size;
						}
						&.column-4 {
							width: 100%;
						}
						&.column-3 {
							width: 75%;
						}
						&.column-2 {
							width: 50%;
						}
						&.column-1 {
							width: 25%;
						}
					}   
					&.mega-is-hover { 
						> a {
							position: relative;
						}
						.dropdown-menu {    
							@include dropdown-visible;  
							z-index: 99999; 
							.dropdown-menu-sub {
								@include dropdown-visible; 
								&.row-product { 
									margin: 0 -10px; 
									flex-wrap: wrap; 
									@include flexbox;
								}  
							} 
						} 
					}
				}
			}
			&.mega-is-hover {
				@include border-radius-reset;
				> .dropdown-inner > a,
				> a {
					color: $nav-link-hover-color;
					@media (min-width: 1025px) {
						&:after {
							width: 100%;
						}
					}   
				} 
				> .dropdown-inner > span.expand,
				> span.expand {
					color: $nav-link-hover-color;
				}
			}
			&:first-child > a:before {
				content: none; 
			}
		}
		> li.mega-menu {
