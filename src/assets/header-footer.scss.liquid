body .m-block-icons {  // m-block-icon menu inside & menu outside
	display: table;
	width: 100%; 
	table-layout: fixed;
	padding: 0;   
	> li { 
		cursor: pointer;
		display: table-cell !important;     // !important fix menu outside
		vertical-align: middle; 
		text-align: center;
		border-bottom: none;
		padding: 10px 20px;   
		@include border-end(1px solid rgba($header-text-mobile-color, 0.1)); 
		> i, .ssw-dropdown-toggle > i {
			font-size: $font-small-icon; 
			color: $header-text-mobile-color; 
		}
		> a, .ssw-dropdown-toggle {
			color: $header-text-mobile-color;
			display: inline-block;
			position: relative;
			padding: 4px; 
		}
		> a {  
			&.waiting {
				height: 100%;
			}
			> span {
				position: relative;
			}
			i {
				font-size: $font-small-icon;
			} 
			svg {
				fill: $header-text-mobile-color !important;
			}
			&.icon-9 svg {
				stroke:$header-text-mobile-color !important;
			}   
			.expand {
				position: static;
				width: auto;
				height: auto;
				line-height: 20px;
				padding: 0;
				display: inline-block;
				&:before {
					content: none;
				}
				i {
					font-size: 14px;
				}
			}
		}
		.number {
			@include icons-number;
			@include ab(n, n, -6px, n, n, -14px); 
			background: $header-icon-mobile-color;
			color: $header-number-on-icon-mobile-color; 
		}    
		&:last-child {
			border: none;
		} 
	} 
	.ssw-dropdown-menu {
		display: none;  
	}
}
.m-close {
	text-align: end;
	border: none;
	background: $main-color;
	z-index: 99999;
	cursor: pointer;
	width: 45px;
	height: 48px;
	line-height: 48px;
	@include ab(absolute, block, 0, n, n, 0); 
	@include opacity(0);
	@include border-radius-circle;
	@include transition(all 0.35s ease 0s);
	i:before {
		@include close-btn;
		color: $body-link-color;
		line-height: 48px;
	}
}
.dropdown-menu {
	@include ab(absolute, block, 100%, 0, n, n);
	float: none;
	text-align: start;
	width: 253px;
	z-index: -999;       
	padding: 0; 
	margin: 0;
	border: none;   
	@include dropdown-hidden;
	background: $nav-dropdown-background; 
	@include border-radius-reset;
	@media (max-width: 1199px) and (min-width: 992px) {
		width: 235px;
	}
	> li {
		> a {
			padding: 3px 20px;
			line-height: 1.42857143;
			&:hover,
			&:focus {
				background-color: transparent;
				color: $nav-dropdown-link-color;
			}
		}
		&.dropdown-submenu {
			> .dropdown-menu {
				@include ab(n, n, 0, 100%, n, n); 
				font-size: $body-font-size;
			}
			&:hover {
				@media (min-width: 992px) {
					> .dropdown-menu {      
						@include dropdown-visible;
					}
				}
			}
		}  
	}
	> .active > a {
		background-color: transparent;
		color: $nav-dropdown-link-color;
		&:hover,
		&:focus {
			background-color: transparent;
			color: $nav-dropdown-link-color;
		}
	}
} 
@media (max-width: 991px) {
	.header-main {   
		.m-cart-icon {
			@include ab(absolute, block, 50%, n, n, 0);
			min-width: 40px;
			text-align: center;
			@include translate(0, -50%); 
			&.waiting {
				background: transparent !important;
			}
			.number {
				@include icons-number;
				background: $header-cart-bg-icon-color;
				color: $header-number-on-icon-color;
			}     
			.mobile-basket {
				position: relative;
				color: $header-cart-icon-color;
				> .demo-icon {
					font-size: 25px;
					color: $header-cart-icon-color;
				}
				> img {
					max-width: 20px;
				}   
			}
		}
	}
	.menu-opened {
		.navbar-responsive-menu span {
			&:nth-of-type(2) {
				@include opacity(0);
			}
			&:nth-of-type(1) {
				@include transform(translate3d(0, 9px, 0px) rotate(45deg));
			}
			&:nth-of-type(3) {
				@include transform(translate3d(0, -5px, 0) rotate(-45deg));
			}
		}
	}
	.navbar-responsive-menu {
		border: none;
		margin: 0;
		padding: 0 !important;
		cursor: pointer;
		z-index: 99;  
		@include ab(absolute, block, 50%, 0, n, n);
		@include translate(0, -50%);
		.responsive-menu {
			width: 24px;
			height: 18px;
			position: relative;
			display: block;
			cursor: pointer;
			span {
				width: 100%;
				height: 2px;
				display: block;
				position: relative;
				background: $text-header-color;
				@include transition(all 0.35s ease-in-out);
				&:nth-of-type(2) {
					top: 5px;
				}
				&:nth-of-type(3) {
					top: 10px;
				} 
			}
		}
	}
	.product-image-inner > .col-md-10 {
		width: 80%; 
	}
	.sb-wrapper {
		margin: 0;
		background: none;  
		border: none;
		height: 100%;
		overflow: auto;
		.sb-widget {
			margin: 0 auto; 
		} 
	}
	.header-menu-wrapper {
		display: none;
	}
	table {
		thead th {
			padding: 10px 15px;
			&.price {
				padding: 10px 15px;  
			}
		}
		td {
			padding: 15px;
		}
	}
	.cart-table table th {
		padding: 10px 15px;
	}
	.left-column-container #related_item {
		margin-top: 40px;
	}
}
@media (max-width: 479px) {
	.header-main .m-cart-icon .mobile-basket {
		> .demo-icon {
			margin: 0;
		}
		.number .money {
			display: none;
		}
		.n-item {
			@include ab(n, n, n, -15px, n, n); 
		}
	}
}      
/*=== mobile-department ===*/
.mobile-department {      
    padding-top: 15px;
    padding-bottom: 15px;  
    @media (max-width: 767px) {
        padding-top: 0px; 
    }
    ul::-webkit-scrollbar {
        height: 6px; 
        background: #eee;
        @include border-radius-25;
    } 
    ul::-webkit-scrollbar-thumb {
        background: $main-color;
        @include border-radius-25;
    }  
    ul {
        @include flexbox();   
        @media (max-width: 812px) {
            -webkit-overflow-scrolling: touch;
        }  
        @media (max-width: 767px) {
            overflow-x: auto; 
            white-space: nowrap;
        } 
        li { 
            list-style: none;
            padding: 0 15px 15px; 
            text-align: center;
            flex: 0 0 auto;
            width: 20%; 
            @media (max-width: 767px) {
                width: 25%;
                padding: 0 calc(15px / 2) 15px; 
            }
            a {
                color: $body-general-color;
                font-size: $body-font-size - 2px;
                font-weight: $font-bold;
                &:hover {
                    color: $body-link-color-hover;
                }  
            } 
            img {
                display: block;
                margin: 0 auto;
                width: auto;
            }    
            .text {
                margin-top: 7px;  
                display: block;
                clear: both;
                text-transform: uppercase; 
            } 
            &.active a {
                color: $main-color;
            } 
        }  
    }
} 
