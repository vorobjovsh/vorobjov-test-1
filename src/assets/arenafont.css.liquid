@font-face {
  font-family: 'arenafont';
  src:  url('{{'arenafont.eot' | asset_url }}');
  src:  url('{{'arenafont.eot' | asset_url }}') format('embedded-opentype'),
    url('{{'arenafont.ttf' | asset_url }}') format('truetype'),
    url('{{'arenafont.woff' | asset_url }}') format('woff'),
    url('{{'arenafont.svg'| asset_url }}') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block; 
}  

[class^="icon-"]:before, [class*=" icon-"]:before { 
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'arenafont' !important;
  speak: none;
  font-style: normal;
  font-weight: normal; 
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  text-decoration: inherit;
  display: inline-block; 

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

