                &:hover {
                    text-decoration: underline;
                }
            }
        }
        &.style-round a {
            @include border-radius-20;
        }
    }
    &.type-banner-ll {
        overflow: hidden;
        .border-widget-brands {
            .widget-partner {
                border: none !important;
            }
            .show-all {
                border-top: 1px solid $body-border-color;
            }
        }
        .widget-image {
            width: calc(100% - 455px);
            float: left;
            @media (max-width: 1440px) {
                width: calc(100% - 300px);
            }
            @media (max-width: 767px) {
                width: 100%;
                float: none;
                height: auto;
            }
            .image {
                @media (min-width: 768px) {
                    height: 720px;
                }
            }
            img {
                @media (min-width: 768px) {
                    height: 720px;
                }
            }
            .text {
                text-align: start;
                @include ab(n, n, n, 13%, n, n);
            }
            .subtitle {
                @media (max-width: 991px) {
                    font-size: 35px;
                }
            }
        }
        .widget-brands {
            width: 455px;
            height: 720px;
            float: left;
            @media (max-width: 1440px) {
                width: 300px;
            }
            @media (max-width: 767px) {
                width: 100%;
                float: none;
                height: auto;
            }
        }
        .widget-partner  {
            .partner-item {
                padding: 0;
                width: 50%;
            }
            &.border-item .partner-item:nth-child(2n) {
                @include border-end(none);
            }
            &.border-item-all .partner-item {
                &:nth-child(-n+2) {
                    border-top: 1px solid $body-border-color;
                }
                &:nth-last-child(-n+2) {
                    border-bottom: none;
                }
            }
        }
        .show-all {
            margin-top: 0;
            padding-top: 35px;
            @media (max-width: 767px) {
                padding: 15px 0;
            }
        }
    }
}
/*=== Sub-page padding-bottom ===*/
#shopify-section-section-partner {
    padding-bottom: 50px;
    @media (max-width: 991px) {
        padding-bottom: 25px;
    }
}
/*=== Section Service bar ===*/
#home-main-content.col-lg-9 .policies-content {
    .tp-next .text {
        width: calc(100% - 25px);
        h6 {
            font-size: $body-font-size - 1px;
        }
    }
    .image {
        width: 25px;
    }
}
.policies-wrapper {
    &.bg-color-none.border-right .policy-item .item.tp-next {
        padding: 0;
        @include margin-end(20px);
        @media (max-width: 1199px) {
            @include margin-end(0);
        }
    }
    &.border-all {
        .policies-content {
            border: 1px solid $body-border-color;
        }
        .policy-item {
            @include border-end(1px solid $body-border-color);
            .tp-next {
                align-items: center;
                justify-content: center;
                padding: 10px 5px 8px;
            }
        }
    }
    &.border-right {
        border-right: none !important;
        .policy-item .item {
            align-items: center;
            padding: 20px 40px 15px;
            @include border-end(1px solid $body-border-color);
            @media (max-width: 1199px) {
                padding: 10px 20px;
            }
        }
    }
    .policy-list {
        overflow: hidden;
        > .policy-item {
            float: left;
            @media (min-width: 992px) {
                width: 20%;
            }
            @media (max-width: 991px) and (min-width: 768px) {
                width: 33.333333%;
            }
            @media (max-width: 767px) and (min-width: 480px) {
                width: 50%;
            }
            @media (max-width: 479px) {
                width: 100%;
            }
        }
    }
    .policy-item {
        .item {
            padding: 17px 0 23px;
            text-align: center;
        }
        .image {
            margin: 0 auto;
            img {
                width: auto;
            }
        }
        .text {
            padding: 16px 5px 0;
            h6 {
                margin: 0 0 3px;
                line-height: 1.2;
                font-weight: 400;
                span {
                    color: $main-color !important; // fixed span color trong admin
                    display: initial;
                }
            }
            span {
                display: block;
                color: $body-general-color;
            }
        }
        .tp-next {
            padding: 7px 0 3px;
            @include flexbox;
            .image {
                margin: 0;
                @include margin-end(15px);
                @include flex(none);
            }
            .text {
                text-align: start;
                padding: 0;
                h6 {
                    font-size: $body-font-size + 1px;
                    margin: 0;
                }
                > span {
                    margin-top: 14px;
                }
            }
        }
    }
    .owl-item {
        padding: 0;
        &:last-child .policy-item {
            @include border-end(none);
            .item {
                @include border-end(none);
            }
        }
    }
}
