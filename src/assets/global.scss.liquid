            + ul {
                padding: 18px 0 0;
                @media (max-width: 991px) {
                    padding: 19px 0 0;
                }
            }
        }
        &.one-column ul li {
            width: 100%;
            float: none;
        }
        ul {
            margin: 0 -5px;
            overflow: hidden;
            li {
                float: left;
                width: 50%;
                padding: 2px 5px;
                @media (max-width: 991px) {
                    float: none;
                    width: 100%;
                }
                a {
                    font-size: $body-font-size + 1px;
                    line-height: 25px;
                    color: $body-link-color;
                    &:hover {
                        text-decoration: underline;
                        color: $body-link-color-hover;
