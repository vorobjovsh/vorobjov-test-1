/* Back to top ---------------------------------*/
#scroll-to-top {
    @include ab(fixed, block, n, n, 10px, 15px);
    z-index: 1000;
    cursor: pointer;
    text-align: center;
    -moz-backface-visibility: hidden;
    @include opacity(0);
    filter: alpha(opacity=0);
    @include transition(all 0.2s ease-out 0s);
    @media (max-width: 1024px) {
        display: none;
    }
    &.on {
        bottom: 10%;
        @include opacity(1);
        filter: alpha(opacity=100);
    }
    i {
        display: none;
    }
    a {
        position: relative;
        display: block;
        border: 1px solid $body-border-color;
        background: $body-background-color;
        color: $body-general-color;
        @include width-btn;
        @include transition(all 0.3s ease-in-out 0s);
        &:before {
            content: "\f106";
            font-size: 22px;
            padding: 0;
            width: 35px;
            height: 35px;
            line-height: 32px;
            text-align: center;
            font-family: $font-family-icon;
            display: block;
            @include transition(all 0.3s ease-in-out 0s);
        }
        &:hover {
            text-decoration: none;
        }
    }
    &:hover a {
        color: $main-color;
    }
}
