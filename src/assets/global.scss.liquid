            @include ab(n, n, n, n, n, 15%);
        }
        .swiper-pagination-bullet {
            display: block;
            margin: 7px auto;
            top: 0;
        }
    }
    &.left-center {
        @include ab(n, n, 50%, 8%, auto, n);
        @include translate(0, -50%);
        @media (max-width: 767px) {
            @include ab(n, n, n, 15%, n, n);
        }
        .swiper-pagination-bullet {
            display: block;
            margin: 7px auto;
            top: 0;
        }
    }
    &.left-bottom {
