            @media (max-width: 767px) {
                padding-bottom: 0;
            }
        }
        .item {
            align-items: center;
            justify-content: center;
            @include flexbox;
            @media (max-width: 767px) {
                padding-bottom: 15px;
            }
            .icon {
                @include padding-end(13px);
                i:before {
                    font-size: 16px;
                }
            }
            .text {
                font-size: $body-font-size - 2px;
            }
        }
    }
    &.style-2 {
        .top-bar-list {
            margin: 0;
            > div {
                padding: 0;
                @include border-start(none);
                &:last-child .item {
                    @media (max-width: 767px) {
                        margin-bottom: 0;
                    }
                }
            }
        }
        .item {
            text-align: center;
            @media (max-width: 767px) {
                margin-bottom: 15px;
            }
            .icon {
                position: relative;
                &:before {
                    content: "";
                    @include ab(absolute, block, 50%, 0, n, n);
                    height: 1px;
                    width: 100%;
                    background: $body-border-color;
                }
                i {
                    background: $announ-icon-bg;
                    z-index: 2;
                    min-width: 100px;
                    display: inline-block;
                    position: relative;
                    &:before {
                        font-size: 55px;
                    }
                }
            }
            .text {
                padding: 0 15px;
                margin-top: 20px;
                font-size: $body-font-size + 5px;
                .heading {
                    line-height: 1.0;
                }
                p {
                    padding-top: 12px;
                }
            }
        }
    }
    &.style-3 {
        .line {
            border-bottom: 1px solid $body-border-color;
            @include flex(1);
            @media (max-width: 991px) {
              display: none;
            }
            &:last-child {
              display: none;
            }
        }
        .top-bar-list {
            align-items: center;
            padding: 15px 0;
            > div:not(.line) {
                max-width: 100%;
                width: auto;
                margin: 0 40px;
                @include border-start(none);
                @include flex(0 0 auto);
                @media (max-width: 1199px) {
                    margin: 0 10px;
                }
                @media (max-width: 991px) {
                    margin: 0;
                }
                @media (max-width: 767px) {
                    @include flex(0 0 100%);
                }
                &:first-child {
                    @include margin-start(0);
                }
                &:nth-last-child(-n+2) {
                    @include margin-end(0);
                }
            }
        }
        .item {
            position: relative;
            align-items: center;
            @include flexbox;
            .icon {
                @include padding-end(20px);
                i:before {
                    font-size: 45px;
                }
            }
            .text {
                font-size: $body-font-size + 1px;
                .heading {
                    line-height: 1.1;
                }
                p {
                    padding-top: 5px;
                }
            }
        }
    }
    &.style-4 {
        .top-bar-list > div {
            @media (min-width: 768px) {
                &:first-child .item {
                    justify-content: flex-start;
                }
                &:last-child .item {
                    justify-content: flex-end;
                }
            }
        }
        .item {
            padding: 14px 0 18px;
            position: relative;
            justify-content: center;
            @include flexbox;
            .icon {
                @include padding-end(20px);
                i:before {
                    font-size: 55px;
                }
            }
            .text {
                font-size: $body-font-size + 1px;
                .heading {
                    line-height: 1.0;
                }
                p {
                    padding-top: 5px;
                }
            }
        }
    }
    &.style-5 {
        .top-bar-list > div {
            padding: 6px 0;
            @media(max-width: 767px) {
                @include flex(0 0 100%);
            }
            &:last-child .item:after {
                content: none;
            }
        }
        .item {
            position: relative;
            padding: 0 40px;
            align-items: center;
            justify-content: center;
            @include flexbox;
            @media (max-width: 991px) {
                padding: 0 20px;
            }
            @media (max-width: 767px) {
                justify-content: start;
            }
            &:after {
                content: "";
                @include ab(absolute, block, 50%, n, n, 0);
                width: 1px;
                height: 12px;
                background: $body-border-color;
                @include translate(0, -50%);
                @media(max-width: 767px) {
                    content: none;
                }
            }
            .icon {
                @include padding-end(18px);
                i:before {
                    font-size: 20px;
                }
            }
            .text {
                font-size: $body-font-size - 1px;
                font-weight: 400;
                p {
                    padding-top: 5px;
                }
            }
        }
    }
    &.style-6 {
        .top-bar-list > div {
            max-width: 100%;
            @include flex(1);
            @include border-start(1px solid $body-border-color);
            @media (max-width: 767px) {
                @include flex(0 0 100%);
                @include border-start(none);
            }
            &:first-child {
                @include border-start(none);
            }
        }
        .item {
            align-items: center;
            justify-content: center;
            padding: 6px 0;
            @include flexbox;
            @media(max-width: 767px) {
                justify-content: start;
            }
            .icon {
                @include padding-end(11px);
                i {
                    font-size: $font-small-icon - 2px;
                }
            }
            .text {
                font-size: $body-font-size - 2px;
                font-weight: 400;
                p {
                    padding-top: 5px;
                }
            }
        }
    }
}
/*=== Announcement-bar in the header ===*/
#shopify-section-announcement-bar {
	.close-notice {
		@include ab(absolute, block, 50%, n, n, 30px);
		cursor: pointer;
