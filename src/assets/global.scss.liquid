        height: 8px;
        margin: 0 7px;
        background: $black;
        outline: none;
        top: -3px;
        @include opacity(1);
        @include border-radius-circle;
        @include transition(all 0.3s ease 0s);
        &:hover,
        &.swiper-pagination-bullet-active {
            background: transparent;
            border: 3px solid $black;
            width: 18px;
            height: 18px;
            top: 0;
        }
    }
    &.center-bottom-2 {
        position: static;
        margin-top: -7px;
