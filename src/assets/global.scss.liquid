}
.product-ratio-false:before {
    content: "";
    display: block;
    padding-bottom: 0;
}
.product-ratio-100:before {
    content: "";
    display: block;
    padding-bottom: 100%;
}
.product-ratio-150:before {
    content: "";
    display: block;
    padding-bottom: 150%;
}
.product-ratio-75:before {
