/*=== Popup newsletter ===*/
.fancybox-slide {
    padding: 0;
    @media (max-width: 767px) {
        @include padding-start(15px);
        @include padding-end(15px);
    }
    #newsletter-popup  {
        width: 810px;
        z-index: 9999;
        margin: 0px auto;
        max-height: 95vh;
        text-align: start;
        @media (max-width: 991px) {
            width: 90%;
            margin: 0 5%;
        }
        .newsletter-popup-content {
            display: table;
            width: 100%;
            height: 100%;
            position: relative;
        }
        .block-image-true {
            .text-box {
                &.position-right {
                    float: right;
                    width: 60%;
                    @include padding-start(60px);
                    @media (max-width: 991px) {
                        @include padding-start(45px);
                    }
                    @media (max-width: 575px) {
                        float: none;
                        width: 100%;
                        @include padding-start(15px);
                    }
                }
                &.position-left {
                    float: left;
                    width: 60%;
                    @include padding-end(60px);
                    @media (max-width: 991px) {
                        @include padding-end(45px);
                    }
                    @media (max-width: 575px) {
                        float: none;
                        width: 100%;
                        @include padding-end(15px);
                    }
                }
                &.position-top {
                    margin-bottom: -40%;
                }
                &.position-bottom {
                    margin-top: -43%;
                }
                &.position-middle {
                    margin-bottom: -43%;
                }
            }
            .block-image {
                img {
                    width: 100%;
                }
                &.image-position-right {
                    float: left;
                    width: 40%;
                    @media (max-width: 575px) {
                        display: none;
                    }
                }
                &.image-position-left {
                    float: right;
                    width: 40%;
                    @media (max-width: 575px) {
                        display: none;
                    }
                }
                &.image-position-top {
                    max-height: 440px;
                    height: 100%;
                    overflow: hidden;
                }
                &.image-position-bottom {
                    max-height: 440px;
                    height: 100%;
                    overflow: hidden;
                }
                &.image-position-middle {
                    max-height: 400px;
                    height: 100%;
                    overflow: hidden;
                }
            }
        }
        .text-box {
            display: block;
            position: relative;
            z-index: 999;
            padding: 40px 45px;
            background: $body-background-color;
            @media (max-width: 767px) {
                padding: 15px;
            }
            .text-box-content {
                margin-bottom: 23px;
                @media (max-width: 767px) {
                    margin-bottom: 15px;
                }
                &:last-child {
                    margin-bottom: 0;
                }
                .form-inline {
                    max-width: 100%;
                }
            }
            .block-text:first-of-type .text {
                line-height: 1.1;
                span {
                    color: $main-color;
                }
            }
            .block-coupon {
                margin-bottom: 10px;
            }
            .bold-text-true {
                font-weight: 600;
            }
            input {
                padding: 13px 20px;
                height: 40px;
            }
            button {
                display: block;
            }
            .button-position-bottom {
                input {
                    width: 100%;
                    display: block;
                    margin-bottom: 10px;
                }
                button {
                    width: 100%;
                }
            }
            .button-position-right {
                form {
                    flex-wrap: wrap;
                    @include flexbox;
                    @media (max-width: 991px) {
                        display: block;
                    }
                    input {
                        width: 60%;
                        @include margin-end(10px);
                        @media (max-width: 991px) {
                            width: 100%;
                            margin-bottom: 20px;
                        }
                    }
                    button {
                        @media (max-width: 991px) {
                            width: 100%;
                        }
                    }
                }
            }
            .block-social {
                text-align: center;
            }
            .widget-social {
                margin: 0;
                &.border-top-true {
                    border-top: 1px solid #eee;
                    padding-top: 20px;
                    margin: 30px 0 0;
                }
                ul li {
                    padding: 0 10px;
                }
            }
            .countdown-content {
                color: $white;
                text-align: center;
                .offer-text {
                    padding: 10px;
                    font-size: 12px;
                }
                .countdown-row {
                    display: table;
                    width: 100%;
                    vertical-align: middle;
                    padding: 0;
                    table-layout: fixed;
                    text-align: start;
                }
                .is-countdown {
                    border-top: 1px solid #333333;
                }
                .countdown-section {
                    margin: 0;
                    width: 25%;
                    display: table-cell;
                    vertical-align: middle;
                    padding: 10px 15px 15px;
                    background: transparent;
                    @include border-end(1px solid #333333);
                    &:last-child {
                        @include border-end(none);
                    }
                    .countdown-amount {
                        background: none;
                        color: $main-color;
                        font-weight: $font-bold;
                        font-size: $size-h1;
                        @media (max-width: 1440px) {
                            font-size: $size-h1 - 10px;
                        }
                        @media (max-width: 991px) {
                            font-size: $body-font-size;
                        }
                    }
                    .countdown-period {
                        color: inherit;
                        font-size: 10px;
                        @media (max-width: 1440px) {
                            font-size: 8px;
                        }
                    }
                }
            }
        }
        .subscribe-result {
            .result-inner {
                text-align: center;
                padding: 45px 40px 40px;
            }
            .text-1 {
                margin: 0 0 30px;
                font-size: $size-h2;
                font-weight: $font-bold;
            }
            .coupon-code {
                margin: 0 0 30px;
                .text-2 {
                    margin-bottom: 15px;
                }
                .code {
                    font-weight: $font-bold;
                }
            }
        }
    }
}
/*=== Newsletter in the section & footer ===*/
.input-bg-white, .input-bg-none {
    .newsletter-container,
    .sb-newsletter {
        form .newsletter-checkbox label:before {
            border: 1px solid $body-border-color;
        }
    }
}
.newsletter-container,
.sb-newsletter {  // Left column market-gray theme
    form {
        max-width: 675px;
        margin: 0 auto;
        position: relative;
        display: block;
        .action-form {
            justify-content: center;
            @include flexbox;
            @media (max-width: 575px) {
                display: block;
            }
            input {
                width: 380px;
                display: block;
                @include margin-end(10px);
                @media (max-width: 575px) {
                    width: 100%;
                    @include margin-end(0);
                }
            }
            button {
                text-transform: uppercase;
                @media (max-width: 575px) {
                    width: 100%;
                    margin-top: 15px;
                }
            }
        }
    }
    .newsletter-checkbox {
        margin: 13px 0 0;
        position: relative;
        display: inline-block;
        text-align: start;
        input {
            @include ab(absolute, block, 0, 0, n, n);
            width: 100%;
            height: 100%;
            cursor: pointer;
            z-index: 2;
        }
        input:checked ~ label {
            &:after {
                content: "\e83d";
                font-size: 14px;
                font-family: $font-family-icon;
                color: $main-color;
                z-index: 1;
                width: 20px;
                height: 20px;
                line-height: 20px;
                text-align: center;
                @include ab(absolute, block, 0, 0, n, n);
            }
        }
        label {
            font-size: $body-font-size - 2px;
            cursor: pointer;
            color: $footer-heading-color;
            margin-bottom: 0;
            font-weight: 400;
            text-align: start;
            &:before {
                content: "";
                display: inline-block;
                vertical-align: middle;
                width: 20px;
                height: 20px;
                background: $input-bg;
                @include margin-end(10px);
            }
            a {
                color: inherit;
                padding: 0 5px;
                text-decoration: underline;
                position: relative;
                z-index: 3;
            }
        }
    }
}
