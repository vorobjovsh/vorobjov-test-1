        border-bottom: 1px solid $body-border-color;
    }
    &.notice-border-bottom .notice-bar-container {
        border-bottom: 1px solid $body-border-color;
    }
    &.notice-border-radius .text-box {
        padding: 5px 139px;
        border: 1px solid $body-border-color;
        @include border-radius-20;
        @media (max-width: 991px) {
            padding: 5px 60px;
        }
        @media (max-width: 767px) {
            padding: 5px 30px;
        }
    }
    &.layout-full .notice-bar-container .border-line:before {
        @include ab(n, n, n, 15px, n, 15px);
    }
    &.layout-right-column .notice-bar-container {
        @media (min-width: 992px) {
            width: 83.33333%;
        }
    }
    .notice-bar-container {
        background-position: center top;
        background-size: cover;
        text-align: center;
        &.bg-image .alignment-left .text-box.flex-layout {
            @include padding-start(45px);
        }
        .alignment-left {
            text-align: start;
        }
        .alignment-left-2 {
            text-align: start;
            .text-box-content {
                @include padding-start(110px);
                @media (max-width: 991px) {
                    @include padding-start(30px);
                }
            }
        }
        .text-box {
            display: inline-block;
            &.flex-layout {
                .text {
                    padding: 0;
                    display: inline-block;
                    vertical-align: middle;
                    p {
                        padding: 0;
                    }
                }
                .btn {
                    display: inline-block;
                    vertical-align: middle;
                    margin: 0 35px;
                    font-weight: 400;
                    text-transform: initial;
                    @media (max-width: 575px) {
                        display: none;
                    }
                    &.bg-none {
                        margin: 0;
                        padding: 0 10px;
                    }
                }
            }
        }
        .text {
            display: block;
            padding: 5px 0 0;
            &:first-child {
                padding-top: 0;
            }
            &.big-size {
                @media (max-width: 1199px) {
                    font-size: 20px !important;
                }
            }
            p {
                margin: 0;
                padding: 0;
                line-height: 1.1;
            }
            .special {
                font-family: $font-special;
                @include padding-end(20px);
            }
            .icon {
                display: inline-block;
                vertical-align: middle;
                @include padding-end(15px);
                i:before {
                    font-size: 35px;
                }
            }
        }
        .btn {
            margin-top: 10px;
            &.bg-none {
                text-decoration: underline;
            }
        }
        .border-line {
            &.alignment-center .text-box-content {
                justify-content: center;
            }
            .alignment-left .text-box {
                @include padding-start(0px);
            }
            .text-box-content {
                overflow: hidden;
                @include flexbox;
            }
            .text-box {
                @include line-left-right;
                justify-content: center;
                @include padding-start(10px);
                @include padding-end(10px);
                &:not(.flex-layout) {
                    flex-direction: column;
                }
            }
        }
    }
    .close-notice {
      display: none;
    }
}

/*=== Notice bar in the header ===*/
#shopify-section-notice-bar {
	display: none;
	.close-notice {
		@include ab(absolute, block, 50%, n, n, 30px);
		cursor: pointer;
        color: $text-other;
        @include translate(0, -50%);
        i {
            display: block;
            &:before {
                @include close-btn;
                color: inherit;
                line-height: normal;
            }
        }
	}
	.notice-bar .notice-bar-container {
		&.bg-image .alignment-left .text-box.flex-layout {
            @include padding-start(0);
        }
		.btn {
			padding: 5px 20px;
			@media (max-width: 991px) {
				display: none;
			}
			&.bg-none {
				padding: 0 10px;
			}
		}
	}
}
/*=== Announcement-bar in the section & header ===*/
.announcement-bar {
    .bar-container {
        background-position: center center;
        background-size: cover;
        background-repeat: no-repeat;
        position: relative;
    }
    .item .text {
        font-weight: $font-bold;
        span {
            text-decoration: underline;
        }
        p {
            margin: 0;
            line-height: 1.3;
            font-weight: 400;
            font-size: $body-font-size;
        }
    }
    .close-notice {
        display: none;
    }
    &.style-1 {
        .bar-container {
