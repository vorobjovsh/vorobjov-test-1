                    @include border-end(none);
                }
            }
            .item {
                padding: 10px 0 85px;
                @media (max-width: 1024px) {
                    padding-bottom: 45px;
                }
                @media (max-width: 767px) {
                    padding-bottom: 15px;
                }
                ul {
                    padding-top: 19px;
                    max-width: 390px;
                    @media (max-width: 1200px) {
                        max-width: 70%;
                    }
                    @media (max-width: 1024px) {
                        max-width: 100%;
                    }
                    li {
                        width: 50%;
                        float: left;
                        a {
                            font-size: $body-font-size - 1px;
                        }
                    }
                }
            }
        }
    }
    .item {
        display: table;
        width: 100%;
        table-layout: fixed;
        vertical-align: middle;
        @media (max-width: 575px) {
            margin-bottom: 15px;
            padding-bottom: 15px;
            border-bottom:  1px solid $body-border-color;
        }
        > div {
            display: table-cell;
            vertical-align: middle;
        }
        .image {
            background: #eee;
            @include border-radius-circle;
        }
        h6 {
            line-height: 1.6;
            font-weight: 400;
            a {
                color: inherit;
                &:hover {
                    text-decoration: underline;
                }
