.container-box {
    padding: 0;
    position: relative;
    > div {
        background: transparent;
        line-height: 1.0;
        padding: 0;
        margin: 0 auto;
        font-family: $font-heading;
        position: relative;
        -webkit-animation-duration: 1s;
        animation-duration: 1s;
        -webkit-animation-fill-mode: backwards;
        -moz-animation-fill-mode: backwards;
        -ms-animation-fill-mode: backwards;
        -o-animation-fill-mode: backwards;
        animation-fill-mode: backwards;
        -webkit-animation-iteration-count: 1;
        animation-iteration-count: 1;
        font-weight: 300;
        b, strong {
            font-weight: $font-bold;
        }
        &.heading {
            margin: 0 auto 1.4%;
            padding: 0;
            line-height: 0.9;
            @media (max-width: 991px) {
                font-size: 3vw !important;
            }
            &.bold-true {
                font-weight: $font-bold;
            }
        }
        &.subheading {
            line-height: 1;
            margin: 0 auto 0.5%;
            @media (max-width: 991px) {
                font-size: 4vw !important;
            }
            @media (max-width: 767px) {
                font-size: 3.5vw !important;
            }
            .big-font {
                font-weight: $font-bold;
            }
            &.bold-true {
                font-weight: $font-bold;
            }
        }
        &.cation {
            margin: 0;
            line-height: 1;
            display: inline-block;
            @media (max-width: 991px) {
                font-size: 4vw !important;
            }
            @media (max-width: 767px) {
                font-size: 3.5vw !important;
            }
            .big-font {
                font-weight: $font-bold;
            }
            span {
                text-decoration: underline;
                color: $main-color;
            }
            &.bold-true {
                font-weight: $font-bold;
            }
        }
        &.caption-link {
            margin: 5% 0 0;
            position: relative;
            @media (max-width: 767px) {
                display: none;
            }
            .slider-button {
                font-weight: $font-bold;
                font-size: $body-font-size + 1px;
                padding: 13px 58px;
                line-height: 26px;
                text-transform: initial;
                display: inline-block;
                @include border-radius-reset;
                @include margin-end(6px);
                @media (max-width: 1024px) {
                    display: none;
                }
                @media (max-width: 767px) {
                    font-size: $body-font-size;
                    padding: 5px 25px;
                }
                &:hover {
                    text-decoration: underline;
                }
            }
        }
        .bg-label {
            position: relative;
            background: #000;
            padding: 20px 15px;
            margin-top: 20px;
            display: block;
            text-decoration: none !important;
            @include border-radius(5px, 0px, 0px, 5px);
            @media (max-width: 991px) {
                @include border-radius(5px, 5px, 5px, 5px);
            }
            &:before {
                content: "";
                @include ab(absolute, n, -1px, n, n, -29px);
                height: 100%;
                border-bottom: 41px solid transparent;
                border-top: 41px solid transparent;
                @include border-start(30px solid #000);
                @media (max-width: 991px) {
                    content: none;
                }
            }
        }
    }
}
.flex-header-left {
    .container-box > div {
        &.heading {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 3.5vw !important;
            }
        }
        &.subheading {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 4.5vw !important;
            }
        }
        &.cation {
            @media (max-width: 1440px) and (min-width: 992px) {
                font-size: 4.5vw !important;
            }
        }
    }
    .banner-item .text .title {
        @media (max-width: 1440px) and (min-width: 992px) {
            font-size: 3vw !important;
        }
    }
}
.wide:not(.flex-header-left) {
    .bc-wrapper.layout-full .swiper-button-white {
        &.swiper-button-prev {
            @include ab(n, n, n, 45px, n, auto);
