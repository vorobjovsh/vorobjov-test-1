/*=== Section List collection V2 ===*/
.lc-container-v2 {
    .list-collection-v2 {
        overflow: hidden;
    }
    h5 {
        color: $text-other;
        font-weight: 400;
        margin-top: 17px;
        display: block;
        width: 100%;
    }
    &.style-image-text {
        .image a {
            display: block;
            margin: 0 auto;
        }
        .list-collection-v2.bg-icon {
            .collection-item {
                padding: 0 20px 20px;
                @media (max-width: 991px) {
                    @include padding-start(10px);
                    @include padding-end(10px);
                }
            }
            .image {
                align-items: center;
                @include flexbox;
                &.it-icon {
                    width: 80px;
                    height: 80px;
                }
                &.it-upload {
                    width: 115px;
                    height: 115px;
                    padding: 21px;
                    text-align: center;
                    @media (max-width: 991px) {
                        width: 85px;
                        height: 85px;
                        padding: 10px;
                    }
                }
            }
        }
        .lc-border-right-full, .lc-border-right-mini, .lc-border-right-rl {
            .type-row {
                @media (min-width: 992px) {
                    justify-content: center;
                }
                .collection-item {
                    @media (max-width: 991px) {
                        border-top: 1px solid $body-border-color;
                        &:nth-child(-n+4) {
                            border-top: none;
                        }
                    }
                }
            }
        }
        .lc-border-right-full .type-row .collection-item {
            @include border-end(1px solid $body-border-color);
            @media (min-width: 992px) {
                padding-top: 30px;
                padding-bottom: 30px;
            }
            @media (max-width: 991px) {
                &:nth-child(4n) {
                    @include border-end(none);
                }
            }
            &:last-child {
                @include border-end(none);
            }
        }
        .lc-border-right-mini .type-row .collection-item {
            @media (max-width: 991px) {
                &:nth-child(4n) .item {
                    @include border-end(none);
                }
            }
            .item {
                @include border-end(1px solid $body-border-color);
            }
            &:last-child .item {
                @include border-end(none);
            }
        }
        .lc-border-right-rl  .type-row .collection-item {
            @media (max-width: 991px) {
                &:nth-child(4n+1) .item {
                    @include border-start(1px solid $body-border-color);
                }
            }
            .item {
                @include border-end(1px solid $body-border-color);
            }
            &:first-child .item {
                @include border-start(1px solid $body-border-color);
            }
        }
        .lc-border-right-none {
            .type-row {
                justify-content: center;
                .collection-item {
                    @media (max-width: 575px) {
                        max-width: 33.33333%;
                        @include flex(0 0 33.33333%);
                    }
                    @media (max-width: 480px) {
                        max-width: 50%;
                        @include flex(0 0 50%);
                    }
                }
            }
            &:not(.bg-icon) .collection-item {
                padding: 10px;
            }
        }
        .type-row {
            flex-wrap: wrap;
            @media (max-width: 812px) {
                -webkit-overflow-scrolling: touch;
            }
            .collection-item {
                padding-top: 20px;
                padding-bottom: 20px;
                @media (max-width: 991px) {
                    max-width: 25%;
                    @include flex(0 0 25%);
                }
                .image {          // fixed image bigger width parent (center image)
                    align-items: center;
                    justify-content: center;
                    @include flexbox;
                }
            }
        }
        .type-row-2 {
            justify-content: center;
            flex-wrap: wrap;
            .collection-item {
                padding: 0 20px;
                margin-bottom: 40px;
                max-width: 16.66667%;
                @include flex(0 0 16.66667%);
                @media (max-width: 991px) {
                    max-width: 20%;
                    @include flex(0 0 20%);
                }
                @media (max-width: 767px) {
                    max-width: 25%;
                    @include flex(0 0 25%);
                }
                @media (max-width: 575px) {
                    margin-bottom: 20px;
                    max-width: 33.33333%;
                    @include flex(0 0 33.33333%);
                }
                @media (max-width: 480px) {
                    max-width: 50%;
                    @include flex(0 0 50%);
                }
                .image.it-upload {
                    width: 115px;
                    height: 115px;
                    padding: 21px;
                }
            }
        }
        .collection-item {
            text-align: center;
            position: relative;
            .item.show-all {
                width: 115px;
                height: 115px;
                padding: 21px;
                text-align: center;
                margin: 0 auto;
                border: 1px solid $body-border-color;
                @include border-radius-circle;
                @media (max-width: 991px) {
                    width: 85px;
                    height: 85px;
                    padding: 10px;
                }
                h4 {
                    padding: 0;
                    display: table;
                    width: 100%;
                    height: 100%;
                }
                a {
                    color: $main-color;
                    font-weight: $font-bold;
                    display: table-cell;
                    vertical-align: middle;
                    &:before {
                        content: "+";
                        font-size: 22px;
                        display: block;
                    }
                    &:hover {
                        text-decoration: none;
                    }
                }
            }
            .image {
                margin: 0 auto;
                @include border-radius-circle;
                i {
                    font-size: 60px;
                }
            }
            h4 {
                color: $body-link-color;
                font-weight: 400;
                padding: 20px 2px 0;
                a {
                    color: inherit;
                }
                &:hover {
                    color: $body-link-color-hover;
                }
            }
            &.textbox-right {
                padding: 0 10px;
                flex-basis: 0;
                flex-grow: 1;
                max-width: 100%;
                .item {
                    align-items: center;
                    flex-wrap: nowrap;
                    @include flexbox;
                    @media (max-width: 812px) {
                        -webkit-overflow-scrolling: touch;
                    }
                    .image {
                        @media (min-width: 992px) {
                            margin: 0;
                        }
                    }
                    h4 {
                        text-align: start;
                        padding: 0;
                        @include padding-start(16px);
                        @include flex(1);
                    }
                }
            }
        }
        .bg-icon .collection-item {
            .image i {
                font-size: 40px;
            }
            h4 {
                padding-top: 17px;
            }
        }
    }
    &.style-text {
        .type-row {
            overflow-x: hidden;
            flex-wrap: wrap;
        }
        .collection-item {
            text-align: start;
            .item {
                background: $bg-pro-v2-list;
                padding: 32px 45px 40px;
                @include padding-end(49px);
                @media (max-width: 991px) {
                    padding: 22px 35px 30px;
                    @include padding-end(39px);
                }
                h4 {
                    padding: 0;
                    position: relative;
                    &:after {
                        content: "\f178";
                        font-size: 20px;
                        font-family: $font-family-icon;
                        color: rgba($body-link-color, 0.2);
                        @include ab(absolute, block, 50%, n, n, 0);
                        @include translate(0, -50%);
                    }
                }
            }
        }
    }
    .btn-show-all {
        text-align: center;
        text-transform: uppercase;
        font-size: $button-size;
        text-decoration: underline;
        margin: 20px 0 0;
        &.line {
            position: relative;
            &:before {
                content: "";
                @include ab(absolute, block, 100%, n, n, n);
                border-bottom: 1px solid $body-border-color;
                width: 100%;
                height: 1px;
                margin-top: -16px;
            }
            > a {
                display: inline-block;
                border: 1px solid $body-border-color;
                text-decoration: none;
                font-weight: 500;
                padding: 5px 20px;
                background: $line-tab-bg;
                position: relative;
            }
        }
    }
}
