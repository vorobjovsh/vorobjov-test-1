        height: auto;
        margin: 0 auto;
        text-align: center;
        visibility: hidden;
        @include opacity(0);
        .img-mobile {
            @media (min-width: 768px) {
                display: none;
            }
        }
        .img-desk {
            @media (max-width: 767px) {
                display: none;
            }
        }
