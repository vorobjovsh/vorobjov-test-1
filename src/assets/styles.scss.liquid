#shopify-section-product-template-v3,
#shopify-section-product-template-v4,
#shopify-section-product-template-v5,
#shopify-section-product-template-v6,     
#shopify-section-product-template-v7 { 
    #shopify-product-reviews .spr-container {
        .spr-header-title, .spr-summary {
            text-align: center;
        } 
        .spr-form-contact, .spr-form-review, .spr-form-actions {
            margin: 0 auto;
        } 
        .spr-form-title {
            text-align: center;
        }  
    }
}
.flex-header-left {
    .page-product  {
        .product-content-wrapper {
            .action-button, .redirect-cart {
                @media (max-width: 1200px) and (min-width: 992px) {
                    float: none;
                    display: block;
                    width: 100%; 
                }    
            }
        }
        &.layout-v5 .product-content-wrapper { 
            .product-info-inner.block-sticked {
                @media (max-width: 1439px) and (min-width: 992px) {
                    width: calc(50% - 165px);   
                }         
            }
        }
    }  
    .page-contact-v1 .block-information ul {
        @media (max-width: 1200px) and (min-width: 768px) { 
            flex-wrap: wrap;  
        }
        li {
            @media (max-width: 1200px) and (min-width: 768px) {
                width: 50%;  
            }
        }  
    }
    .cata-toolbar {
        .sort-by {
            @media (max-width: 1199px) {
                @include padding-start(10px); 
            }   
        }
        .cata-title h1 {
            @media (max-width: 1199px) {
                font-size: $size-h1 - 10px; 
            }  
        }
    }  
    &.product-v5.wide-padding .layout-v5 .product-content-wrapper {
        .product-info-inner.block-sticked {  
            @media (min-width: 992px) {
                width: calc((100% - #{$width-header-left} - 30px) / 2 - 10px);  
            }   
            @media (min-width: 1200px) {  
                width: calc((100% - #{$width-header-left} - 140px) / 2 - 10px);   
            }                
        }
    }  
}  
.product-v5.wide-padding .layout-v5 .product-content-wrapper { 
    .product-info-inner.block-sticked {  
        @media (min-width: 992px) {
            width: calc((100% - 30px) / 2 - 10px);
        }   
        @media (min-width: 1200px) {  
            width: calc((100% - 140px) / 2 - 10px); 
        }            
    }
}
.btn-round .page-product {
    .product-image-inner .slider-main-image .slick-arrow {
        @include border-radius-circle;
    }   
    .product-content-wrapper .shopify-payment-button button {
        @include border-radius-50;
    } 
}    
.page-product { 
    padding: 30px 0 78px;   
    @media (max-width: 991px) {
        padding: 30px 0 40px;  
    }
    @media (max-width: 767px) {
        padding: 15px 0 30px;
    } 
    
    &.layout-v2 {  
        @media (min-width: 992px) {
            padding-bottom: 80px; 
        }
        .product-content-wrapper {
            h1[itemprop="name"] {
                font-size: $product-detail-title-size + 10px; 
                margin: 0 0 8px; 
                @include padding-end(50px);
                @media (max-width: 991px) { 
                    font-size: $product-detail-title-size;
                }
                @media (max-width: 767px) { 
                    font-size: $product-detail-title-size - 4px;  
                } 
            } 
            .product-info-inner {
                @media (min-width: 768px) {
                    margin-top: -10px; 
                }
                .product-label {
                    top: 9px;
                } 
            } 
            @media (min-width: 768px) {
                .product-vendor {
                    margin: 0 0 23px; 
                }
                .group-cw {
                    margin: 40px 0 10px; 
                }
                .qty-add-cart {
                    padding-top: 13px;   
                }
            }
        } 
        .product-simple-tab {
            @media (min-width: 992px) {
                padding-bottom: 80px;
            } 
        }
    }
    &.layout-v3 {
        padding-top: 0;
        @media (min-width: 992px) {
            padding-bottom: 79px;  
        }  
        #product-image {
            margin: 0;
        }  
        .product-image-inner {
            .slick-list.draggable {
                margin: 0 -10px;    
            }    
            .slider-main-image {
                width: 100%; 
                .slider-for-03:not(.slick-slider) {
                    .slick-item { 
                        width: 33.33333%;
                        display: inline-block;   
                    }  
                }  
                .slick-item {
                    padding: 0 10px; 
                } 
                .slick-arrow {
                    @media (min-width: 1025px) {
                        @include opacity(1);
                        visibility: visible;
                        background: rgba($body-background-color, 0.3);
                        &:hover {
                            background: $body-background-color; 
                        }    
                    }
                }
            } 
        } 
        .product-content-wrapper {
            .product-info {
                clear: both;
                .product-info-inner { 
                    margin: 0 auto; 
                    text-align: center;
                    padding: 51px 15px 0;    
                    @media (max-width: 767px) {
                        padding: 30px 15px 0;  
                    }
                    .product-label {
                        z-index: 1000;    
                        @include ab(n, block, -10px, 50%, n, n); 
                        @include translate(-50%, 0);
                    }   
                }    
            }
            h1[itemprop="name"] {
                max-width: 600px; 
                text-align: center; 
                font-size: $product-detail-title-size + 10px; 
                margin: 0 auto 10px;      
                @media (max-width: 991px) { 
                    font-size: $product-detail-title-size; 
                }
                @media (max-width: 767px) { 
                    font-size: $product-detail-title-size - 4px;  
                }
            }
            .product-vendor {
                margin: 0 0 22px;  
                @media (max-width: 991px) {
                    margin: 0 0 12px; 
                }  
            }
            .product-price {
                text-align: center;  
            }
            .rating-links {
                display: inline-block; 
                margin: 0 0 25px; 
                @media (max-width: 991px) {
                    margin: 0 0 15px;
                }   
            }
            .group-cw {
                margin: 23px 0 24px;
                @media (max-width: 991px) {
                    margin: 13px 0 14px;
                }   
                .swatch {
                    margin: 0;
                    padding: 0 27px;
                    @media (max-width: 991px) {
                        padding: 0 15px;
                    }
                    @media (max-width: 767px) {
                        padding: 0 10px;  
                    }
                    @media (max-width: 575px) {
                        width: 100%; 
                        padding: 0 0 10px;   
                        margin: 0 auto;
                        text-align: center;  
                    }
                    &:first-child {
                        @include padding-start(0);
                    }
                    &:last-child {
                        @include padding-end(0);
                    }  
                    .header {
                        text-align: start;  
                        @media (max-width: 575px) {
                            text-align: center;
                        }
                    }
                    .swatch-element {
                        @media (max-width: 575px) {
                            display: inline-block; 
                            float: none;
                        }
                    }
                }
            }
            .product-form {
                margin-bottom: 26px;  
                justify-content: center; 
                @include flexbox;
                @media (max-width: 1200px) {
                    display: block;  
                }     
                .options {
                    justify-content: center;
                    flex-wrap: wrap;
                    @include flexbox; 
                }    
                .variants-wrapper {
                    justify-content: center;
                    flex-wrap: wrap;
                    @include flexbox;
                    .selector-wrapper {
                        padding: 0 10px; 
                        margin-bottom: 15px;
                        &:first-child {
                            @include padding-start(0);
                        } 
                        select { 
                            min-width: 80%; 
                        }
                    }
                }    
            } 
            .shipping-time {
                margin-bottom: 26px;
                @media (max-width: 767px) { 
                    margin: 0 0 15px;
                }     
            } 
            .qty-add-cart {
                justify-content: center;
                padding: 5px 0 0;
                @include padding-start(8px);
                @include flexbox; 
                @media (max-width: 991px) {
                    max-width: 530px;   
                    margin: 0 auto;
                    @include padding-start(0);
                } 
                @media (max-width: 767px) {
                    display: block; 
                }  
                @media (max-width: 575px) {
                    max-width: none; 
                }
                .quantity-product {
                    margin-bottom: 0;
                    @media (max-width: 575px) {
                        margin: 0 auto 15px;
                        float: none; 
                    }   
                }   
                .action-button {
                    margin-bottom: 0;
                    @media (min-width: 768px) {
                        width: auto; 
                        @include margin-end(10px);
                    }
                    button {
                        @media (min-width: 1025px) { 
                            min-width: 282px; 
                        }    
                    }
                }
                .shopify-payment-button {
                    margin: 0;
                    @media (max-width: 767px) {
                        margin: 15px 0 0;  
                        width: 100%;
                        float: left;  
                        display: block;  
                    }
                    button {
                        @media (min-width: 1025px) {  
                            min-width: 282px; 
                        }  
                    }
                }
            }        
            .pre-order, .add-to-wishlist, .add-to-compare, #widget-fave-html {
                display: inline-block; 
                float: none; 
                padding: 0 18px; 
                @media (max-width: 767px) {
                    padding: 0 5px;
                }
                @media (max-width: 575px) {
                    padding: 0 0 8px;
                    display: block; 
                }
                i, svg {
                    float: none; 
                }   
            }
        } 
        .inventory-quantity {
            max-width: 600px;
            margin: 0 auto;
        }
        .detail-deal {
            max-width: 600px;
            margin: 15px auto 0;
        }  
        .people-in-cart {
            justify-content: center; 
        }
        .share-links.social-sharing {
            padding: 20px 0 0;
            .sharethis-inline-share-buttons {
                text-align: center !important;  
            } 
        } 
        .product-sku-stock li {
            text-align: center; 
        }
        .deadline { 
            display: flex;
            justify-content: center;
            align-items: center;
        } 
    }
    &.layout-v4 {
        padding: 60px 0 0;  
        margin-bottom: 75px;
        @media (max-width: 991px) {
            padding: 30px 0 0; 
            margin-bottom: 40px;
        }     
        @media (max-width: 767px) {
            margin-bottom: 35px;  
        }
        .product-image-inner {
            .meta-image360 {
                @include ab(n, n, n, n, n, 0);
            }
            .meta-video {
                @include ab(n, n, 0, n, n, 0);
            } 
            .slider-thumbs-01 .slick-item { 
                border: 1px solid $body-border-color;  
            }  
        }  
        .product-info {
            .product-info-inner {
                margin: 0; 
                @include padding-start(0);
            }
            .box-content { 
                position: relative;
                text-align: start; 
                background: $body-background-color; 
                padding: 38px 38px 15px;  
                margin-bottom: 24px;  
                @media (max-width: 1024px) {
                    padding: 25px 25px 15px;
                }
                @media (max-width: 767px) {
                    padding: 15px;     
                }  
            }
        }
        .product-content-wrapper {
            .product-info-inner {
                text-align: center;
                .product-label {
                    top: 0;
                }
            } 
            .action-button .btn, .shopify-payment-button button {
                padding: 10px 15px; 
                @media (max-width: 1440px) and (min-width: 992px){
                    font-size: $button-size;
                }
            } 
            .qty-add-cart {
                @media (min-width: 768px) {
                    padding: 4px 0 8px; 
                }   
            }
            .group-cw {
                margin: 14px 0 0;  
            } 
            .add-to-compare, .add-to-wishlist {
                display: inline-block; 
            }
            .add-to-compare {
                @include margin-end(38px);
                @media (max-width: 1024px) {
                    @include margin-end(20px);
                }
                @media (max-width: 767px) { 
                    @include margin-end(10px);
                }
            }
            .pre-order {
                padding-bottom: 32px;  
                display: inline-block; 
                float: none;
                @media (max-width: 767px) {
                    padding-bottom: 8px;
                }     
            }
        }
        .share-links.social-sharing {
            .sharethis-inline-share-buttons {
                text-align: center !important;  
            } 
        }
        .review-item {
            @media (min-width: 992px) {
                padding-bottom: 80px; 
            }
        }    
    }
    &.layout-v5 {
        @media (min-width: 992px) {
            padding-top: 20px; 
        }
        .product-image-inner {
            .item {
                margin-bottom: 20px;
                &:last-child {
                    margin-bottom: 0; 
                }   
            }
            .stick-gallery-image {
                @media (max-width: 991px) {
                    display: none; 
                }
            }
            .slider-main-image {
                display: none;
                @media (max-width: 991px) {
                    display: block;
                    width: 100%;
                    float: none;  
                }
                .slick-list.draggable {
                    margin: 0 -10px;
                    .slider-for-03:not(.slick-slider) {
                        .slick-item { 
                            width: 33.33333%;
                            display: inline-block;   
                        }  
                    }   
                    .slick-item {
                        padding: 0 10px; 
                    }  
                }
            }
        }
        .product-content-wrapper { 
            #product-image {
                margin: 0;
                @media (min-width: 992px) {
                    padding-top: 10px; 
                }
            }
            h1[itemprop="name"] {
                font-size: $product-detail-title-size + 10px;
                @include padding-end(50px);
                @media (min-width: 992px) {
                    margin-bottom: 8px;   
                } 
                @media (max-width: 991px) { 
                    font-size: $product-detail-title-size; 
                }
                @media (max-width: 767px) { 
                    font-size: $product-detail-title-size - 4px;  
                }
            }
            .product-info-inner {
                margin-top: 0;  
                @media (max-width: 991px) {
                    padding: 30px 0 0; 
                }    
                &.block-sticked {
                    @media (min-width: 992px) { 
                        z-index: 99;  
                        @include ab(fixed, n, 0, n, n, n);
                        height: 100%;     
                        width: calc((980px - 30px) / 2 - 10px);     
                    }  
                    @media (min-width: 1200px) {
                        width: calc((1200px - 30px) / 2 - 10px);  
                    }  
                    @media (min-width: 1440px) { 
                        width: calc((#{$container-width} - 30px) / 2 - 10px);
                    }      
                    &.hide-temporary {
                        @media (min-width: 992px) {
                            @include ab(absolute, block, auto, 15px, 0, 0); 
                            height: auto;
                            overflow: inherit;
                            width: auto; 
                        }
                    }   
                } 
                .product-label {
                    top: 9px; 
                } 
            }   
            .product-vendor {
                margin: 0 0 23px;
                @media (max-width: 767px) {
                    margin: 0 0 14px;
                } 
            } 
            .product-price {
                margin-bottom: 10px;   
            }
            .group-cw {
                margin-top: 46px;
                @media (max-width: 991px) {
                    margin-top: 35px;
                }
                @media (max-width: 767px) {
                    margin-top: 20px;
                }  
            } 
        }
        .product-simple-tab {
            padding: 35px 0;
            @media (max-width: 767px) {
                padding: 20px 0;   
            } 
            #product-accordion {
                .panel-product-accordion {
                    text-align: start;
                }
                .panel-collapse {
                    padding-top: 19px;  
                }
                .panel-title {
                    @media (max-width: 767px) {
                        font-size: $size-h4 - 5px; 
                    } 
                }
            }
        }
        #shopify-product-reviews .spr-container { 
            .spr-form-label { 
                width: auto; 
            }  
            .spr-form-input,  
            .spr-form-input-email {
                width: 100%;
                float: left;
            }
            .spr-form-input-textarea  {
                max-width: none;   
            }
            .spr-form-review-rating .spr-starrating {
                @include margin-start(0);  
            }
            .spr-form-actions .btn {
                @include margin-start(0);  
            }
            .spr-review {
                &:last-child {
                    border-bottom: none;  
                }
                .spr-review-header {
                    min-width: none;
                }
                .spr-review-content {
                    width: 100%;
                    padding: 10px 0 0;
                }
            }
        }
    }
    &.layout-v6 {
        .product-image-inner {
            .slider-main-image {
                width: 100%;
                float: left;
            }
            .slider-thumbs-01 {
                width: 100%;
                float: left;     
                .slick-list.draggable {
                    margin: 0 -5px;    
                    .slick-slide { 
                        padding: 10px 5px 0;  
                    }   
                }
            }
        }
        .product-content-wrapper {
            .group-cw {
                @media (min-width: 768px) {
                    margin-top: 33px;  
                } 
            }  
            .qty-add-cart {
                @media (min-width: 768px) {
                    padding-top: 12px; 
                }  
            } 
        }
    }
    &.layout-v7 {
        padding-bottom: 85px;
        padding-top: 22px;  
        @media (max-width: 991px) {
            padding-bottom: 45px;
        }    
        @media (max-width: 767px) {
            padding-bottom: 30px;
        }                   
        .product-top-content {
            display: block;
            position: relative;
            text-align: center; 
            h1[itemprop="name"] {
                max-width: 600px; 
                margin: 0 auto 10px;   
                text-align: center;  
                font-size: $product-detail-title-size + 10px;
                @media (max-width: 991px) { 
                    font-size: $product-detail-title-size; 
                    max-width: 500px;
                }
                @media (max-width: 767px) { 
                    font-size: $product-detail-title-size - 4px;  
                    max-width: 400px;    
                }    
            }
            .product-label {
                @include ab(n, n, 10px, auto, n, 0);
                @media (max-width: 767px) {
                    top: 5px; 
                }
            }
            .rating-links {
                display: inline-block; 
                margin-bottom: 21px;    
            } 
            .product-vendor {
                margin: 0 0 12px;   
            }    
        }
        .product-image-inner .slider-main-image {
            width: 100%; 
            .slider-for-07:not(.slick-slider) {    
                .slick-item { 
