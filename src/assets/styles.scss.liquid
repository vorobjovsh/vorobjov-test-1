                        .spr-badge-caption {
                            display: inline-block; 
                        }
                    }
                    .product-price {
                        padding: 19px 0 0;  
                        @media (max-width: 575px) {
                            padding: 8px 0 0;
                        }        
                    } 
                    .price-sale, .price-compare, .price {
                        @media (min-width: 992px) {
                            font-size: $product-price-size + 4px;
                        } 
                    } 
                    .product-list-mode-content {
                        display: block;
                        padding: 31px 0 0;
                        @media (max-width: 767px) {
                            padding: 16px 0 0;
                        } 
                        @media (max-width: 575px) {
                            padding: 8px 0 0;
                        } 
                        .plm-product-button {
                            display: none;
                        }  
                        .plm-add-cart {
                            display: none;
                            @media (max-width: 767px) {
                                display: block;
                                margin-top: 20px;   
                            } 
                            @media (max-width: 575px) {
                                margin-top: 10px;
                                font-size: $body-font-size - 3px;
                            } 
                        } 
                        .plm-button {
                            display: none;
                            @media (max-width: 767px) {
                                padding-top: 12px;
                                display: block;
                            }   
                            > div a {
                                font-size: $body-font-size - 3px; 
                                text-transform: uppercase;
                                font-weight: $font-bold;
                                span {
                                    text-decoration: underline;
                                    @include margin-start(11px);
                                }      
                            } 
                        } 
                    }
                    .plm-description {
                        display: block;
                        padding: 18px 0 0;  
                        font-size: $body-font-size - 1px;
                        @media (max-width: 575px) {
                            display: none;
                        }  
                    }
                    .product-code, .avaibility {
                        font-size: $body-font-size - 3px;
                        text-transform: uppercase;
                        span {
                            min-width: 90px;
                            display: inline-block;
                        }    
                    }   
                    .avaibility {
                        padding: 4px 0 0;
                    }
                }
                .plm-add-cart {
                    @include product-add-cart; 
                }       
                .product-list-mode-content-v2 {
                    width: 250px; 
                    text-align: center;
                    @include padding-start(30px);
                    @media (max-width: 1440px) {
                        width: 30%;
                    }
                    @media (max-width: 767px) {
                        display: none;
                    } 
                    .plm-product-compare {
                        padding-top: 15px; 
                    }
                    .plm-product-wishlist {
                        padding-top: 8px; 
                        .ssw-faveiticon i span {
                            display: inline-block;
                        } 
                    }
                    .plm-product-compare, .plm-product-wishlist {
                        color: $body-link-color;
                        a { 
                            font-size: $body-font-size - 3px; 
                            text-transform: uppercase;
                            font-weight: $font-bold;
                            span {
                                text-decoration: underline;
                                @include margin-start(11px);
                            }
                            &:hover {
                                text-decoration: none;
                                svg {
                                    fill: $body-link-color-hover !important;
                                }  
                            }    
                        }
                        &:hover {
                            color: $body-link-color-hover; 
                        }
                    }
                }  
            } 
        }
    }
} 
.fix-boxed .category-grid-view {
    @media (min-width: 1440px) {
        max-width: $container-width;  
    }     
}
#page-body.wide-padding {
    .category-grid-view {
        max-width: 100%;
        @media (min-width: 1200px) { 
            @include padding-start(70px);     
            @include padding-end(70px);
        }  
    }
}
.category-grid-view {  
    @include width-boxed;  
} 
.category {    
    margin: 0;
    padding: 0 0 20px;       
    position: relative;   
    @include transition(all 0.35s ease 0s);
    .category-name {                
        margin: 0 auto; 
        font-size: $body-font-size + 3px;       
        color: $body-link-color; 
        text-transform: uppercase;
        font-weight: $font-bold;  
        @media (max-width: 767px) {
            font-size: $body-font-size; 
        }      
        &:hover {
            @media (min-width: 1025px) {
                color: $main-color;
            }      
        } 
    }
    .category-image {
        margin: 0 auto 10px;
        position: relative;
        &:before, &:after {
            content: "";
            background: $main-color;
            @include ab(absolute, block, n, n, -4px, -4px);
        }
        &:before {
            width: 2px;
            height: 0;
            @include transition(height 0.4s ease-in-out);
        }  
        &:after {
            width: 0; 
            height: 2px; 
            @include transition(width 0.4s ease-in-out);
        } 
        > a {
            display: block;
            position: relative; 
            overflow: hidden;
            img {
                @include transition(transform 0.4s ease-in-out); 
            }
        } 
    }
    &:hover {
        @media (min-width: 1025px) {
            .category-image {
                &:before {
                    height: 100%;
                }
                &:after {
                    width: 100%;
                }
                > a img {
                    @include transform(scale(1.1));
                }     
            }
        }
    }      
}
