                    @media (max-width: 767px) {
                        width: 42%;
                    }
                    @media (max-width: 370px) {
                        width: 100%;
                    }
                    .product-button, .product-add-cart, .detail-deal {
                        display: none;
                    }    
                }
                .product-content {
                    width: calc(100% - 520px);
                    padding: 0 30px;
                    position: relative;
                    @include border-end(1px solid $body-border-color);
                    @media (max-width: 1440px) {
                        width: 39%;
                    }
                    @media (max-width: 767px) {
                       width: 58%; 
                       padding: 0;   
                       @include padding-start(20px);
                       @include border-end(none);
                    }   
                    @media (max-width: 370px) {
                        width: 100%;
                        padding: 15px 0 0;
                    }      
                    .pc-inner {
                        position: static;
                    }    
                    .list-v2-label {
                        @include ab(absolute, block, 0, 30px, n, n);
                        @media (max-width: 767px) {
                            display: none;
                        }   
                        .label-in-stock {
                            display: inline-block;          
                            clear: both;
                            text-align: center;
                            padding: 4px 5px;        
                            border: none;        
                            z-index: 1;    
                            color: $white;
                            background: $black; 
                            font-size: 11px;  
                            text-transform: uppercase;  
                            font-weight: $font-bold;
                        }  
