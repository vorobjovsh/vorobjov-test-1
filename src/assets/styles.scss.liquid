.cart-table {
    margin-bottom: 50px;
    width: 100%;
    overflow-x: auto;   
    @media (max-width: 767px) {
        margin-bottom: 0;
    }  
    table {
        width: 100%;   
        tbody tr {
            &.last {
                @media (max-width: 575px) {
                    border-bottom: none;
                }
