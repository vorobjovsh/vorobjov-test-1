                } 
                .action-bundle {
                    padding: 15px 0 0; 
                    display: block;
                    float: none;
                    width: 100%;
                }
            }
        } 
    }
    .tab-content {
        padding: 50px 0;  
        @media (max-width: 991px) {
            padding: 20px 0; 
        } 
        #tabs-description {
            line-height: 22px;
            h5 { 
                color: $body-link-color;      
            }
            h1, h2, h3, h4, h5, h6 {      
                font-weight: normal; 
                margin: 0 0 23px;    
                line-height: 40px;  
            }
