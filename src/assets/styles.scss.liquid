            thead {
                display: none;
            }
            .cart-item {
                padding-top: 15px;
                margin-bottom: 15px; 
                border-top: 1px solid $body-border-color; 
                display: table;
                width: 100%;
                vertical-align: middle;
                &:first-child {
                    border-top: 0;
                }  
            }  
            td {
                padding: 0; 
                display: table-cell;
                &.item-qty {
                    width: 100%; 
                    display: table-row;
