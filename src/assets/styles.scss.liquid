                } 
                &.item-image {
                    display: none;  
                }
                &.item-name { 
                    display: table-cell;
                    padding: 15px 0;
                    .item-image {
                        @include padding-end(15px);
                    }    
                    .item-title {
                        padding-top: 10px;
                        a { 
                            font-size: $body-font-size;
                        }
                        .wrap-item-variant {
                            margin: 0; 
                            .item-variant {
                                font-size: $body-font-size;
                                padding: 0;
                                @include padding-end(10px);
                            }
                        }
                    }
                    .item-price {
                        display: block; 
                        font-size: $body-font-size;
                    }
                }
                &.item-price {
                    display: none;
                }
                &.item-total {
                    display: none;
                }
            }
            .action {
                display: none;
            } 
        }
    }
