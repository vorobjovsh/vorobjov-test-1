.srp-filter-wrapper {  
    display: table;
    width: 100%;
    margin-bottom: 15px;
    > a {
        line-height: 18px;
        font-size: $body-font-size;
        display: inline-block;
        @include margin-end(15px);
        @include padding-end(15px);
        @include border-start(1px solid $body-border-color);
        &:before {
            content: "\e8cc";
            font-family: $font-family-icon;
            font-size: 12px;
            @include margin-end(7px);         
        }
        &:hover {
            &:before {
                content: '\e8cc'; 
            }
        }
    }
    .srp-filter {
        display: inline-block;
        li {
            @include margin-end(15px);
            span {
                display: none;        
            }
            a {
                line-height: 18px;
                font-size: $body-font-size;
                &:before {
                    content: "\e8cc";
                    font-family: $font-family-icon;
                    font-size: 12px;
                    @include margin-end(7px);
                }
                &:hover {
                    &:before {
                        content: '\e8cc';       
                    }
                }
            }
        }
    }
}     
.sb-widget { 
    .sb-filter-tag {   
        overflow: auto;
        li {
            margin: 0 0 13px; 
            line-height: 20px;
            span {
                position: relative;
                display: inline-block;   
                font-size: $body-font-size - 1px;   
                font-weight: $font-bold; 
                text-transform: uppercase; 
                word-break: break-word; 
                color: $body-general-color;  
                @include padding-start(30px);   
                &:before {
                    content: " "; 
                    border: 1px solid $bg-other; 
                    background: $bg-other; 
                    width: 20px;
                    height: 20px;
                    @include ab(absolute, block, -2px, 0, n, n);   
                    @include transition(all 0.35s ease 0s);
                }   
                &:after {
                    content: "\e83d";
                    font-size: 10px;
                    font-family: $font-family-icon; 
                    width: 20px;
                    height: 20px;
                    text-align: center;  
                    line-height: 18px; 
                    @include opacity(0); 
                    visibility: hidden; 
                    @include ab(absolute, block, 0, 0, n, n);
                    @include transition(all 0.35s ease-in-out); 
                } 
                &:hover {
                    @media (min-width: 1025px) {
                        color: $body-link-color-hover; 
                        text-decoration: underline;  
                        &:after {
                            @include opacity(1);
                            visibility: visible;  
                        }                 
                    }               
                }
            }
            &.active > span { 
                color: $body-link-color-hover; 
                text-decoration: underline;  
                &:after {
                    @include opacity(1);
                    visibility: visible;  
                }      
            }  
        } 
    } 
    .sbw-filter {  
        overflow: hidden; 
        .sbf-title { 
            padding: 14px 15px;
            margin: 0 0 25px;  
            .clear-filter {
                text-transform: capitalize;
                cursor: pointer;
                font-weight: 400;    
            }   
            span {                
                cursor: pointer;        
                i {
                    display: none;
                    &:before {
                        content: none;
                    }         
                } 
            }
        }
        > ul,
        > o {
            list-style: none;
            margin: 0;
            padding: 15px; 
            li {
                margin: 8px 0;
                a:hover {
                    color: #888;
                } 
            }
        }
        > div {    
            .row {
                margin-bottom: 15px;
                &:last-child {
                    margin-bottom: 0;
                }
            }        
            .sbf-title {
                color: $main-color;
                margin: 0;      
                padding: 0;       
                font-size: $body-font-size - 2px;  
                font-weight: $font-bold;          
            }   
            .advanced-filters {
                list-style: none;
                padding: 16px 0 0; 
                max-height: 290px;
                overflow: auto; 
                list-style: none;
                &.afs-color {
                    margin: 0 -2px;          
                }      
                > li {
                    .inner > a {  
                        position: relative;   
                        color: $body-general-color; 
                        display: inline-block;      
                        font-size: $body-font-size - 1px; 
                        font-weight: $font-bold;  
                        text-transform: uppercase;     
                        &:hover {
                            color: $body-link-color-hover;           
                        } 
                    }
                    &.advanced-filter:not(.af-color) {
                        line-height: 20px;
                        margin-bottom: 11px;   
                        a {
                            &:before {
                                content: " "; 
                                border: 1px solid $bg-other; 
                                background: $bg-other; 
                                width: 20px;
                                height: 20px;
                                display: inline-block;
                                vertical-align: middle; 
                                @include margin-end(10px);    
                                @include transition(all 0.35s ease 0s);
                            }  
                            &:after {
                                content: "\e83d";
                                font-size: 10px;
                                font-family: $font-family-icon; 
                                width: 20px;
                                height: 20px;
                                text-align: center;  
                                line-height: 18px; 
                                @include opacity(0); 
                                visibility: hidden; 
                                @include ab(absolute, block, 2px, 0, n, n);
                                @include transition(all 0.35s ease-in-out); 
                            }  
                        }
                        &.active-filter .inner > a {  
                            color: $body-link-color-hover; 
                            &:after {
                                @include opacity(1);
                                visibility: visible;  
                            }         
                        }   
                    }
                    &.af-color {               
                        margin: 0 2px;
                        padding: 1px;
                        width: 28px;
                        height: 28px;
                        line-height: 28px;                  
                        position: relative; 
                        cursor: pointer;
                        float: left;      
                        display: block;         
                        border: 2px solid transparent;   
                        @include transition(all 0.35s ease 0s);  
                        .inner {
                            width: 100%;
                            height: 100%;
                            border: 1px solid $body-border-color;  
                        }       
                        a {                            
                            padding: 0; 
                            position: relative;
                            display: block;    
                            font-size: 0;
                            width: 100%;   
                            height: 100%; 
                        } 
                        &:hover {
                            @media (min-width: 1025px) {   
                                border-color: #bcbcbc;                   
                            } 
                        }
                        &.active-filter { 
                            border-color: #bcbcbc;       
                        }  
                    } 
                }
            }    
        }  
    }
    .filter-order-form {
        @include drawer-sidebar;   
    }
    &.filter-sidebar { 
        &.position-sidebar {
            &.style-normal .sb-filter-wrapper .f-close {
                @media (min-width: 992px) {
                    display: none;
                }
            }  
            &.style-accordion .sb-filter-wrapper {
                .accordion {
                    .sbf-title {
                        position: relative;
                        cursor: pointer;
                        padding: 3px 0;    
                        @include padding-end(20px);        
                        &:before {
                            content: "\e873";
                            font-family: $font-family-icon;
                            font-size: $body-font-size - 2px; 
                            width: 20px;  
                            height: 20px;
                            line-height: 20px;
                            text-align: center;
                            font-weight: 400;
                            @include ab(absolute, block, 2px, n, n, 0);
                            @include transition(all 0.35s ease 0s);
                        }             
                    }
                    &.accordion-active .sbf-title:before {
                        content: "\e874"; 
                    }  
                }        
                .f-close {
                    @media (min-width: 992px) {
                        display: none;
                    }
                }
            }
        }
        &.position-body {
            &.style-toggle {
                @include toggle-sidebar; 
                .sb-filter-tag {
                    padding-bottom: 10px;
                    letter-spacing: -0.31em;
                    vertical-align: top;
                    li {
                        display: inline-block;
                        letter-spacing: 0;
                        vertical-align: top;
                        margin: 0 0 13px;
                        line-height: 20px; 
                        padding: 0;
                        @include padding-end(10px);
                        @media (min-width: 1200px) {
                            width: 16.666667%;
                        }
                        @media (max-width: 1199px) and (min-width: 992px) {
                            width: 20%;
                        }
                        @media (max-width: 991px) and (min-width: 768px) {
                            width: 25%;
                        }
                        @media (max-width: 767px) and (min-width: 556px) {
                            width: 33.333333%;
                        }
                        @media (max-width: 555px) {
                            width: 50%;
                        }
                        &:first-child {
                            display: block;
                            width: 100%;
                        } 
                    }
                }
            }
            &.style-drawer {
                @include drawer-sidebar; 
                display: block !important; 
                .sbw-filter {
                    margin-bottom: 20px;  
                    > div .advanced-filters {
                        max-height: none; 
                    } 
                }  
            }
        }
    }  
}
