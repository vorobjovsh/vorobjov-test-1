                margin-bottom: 15px; 
            }
            &:last-of-type {
                margin-bottom: 0;
            }
        } 
        .description-image {
            margin: -50px 0; 
            @media (max-width: 1199px) {
                padding-bottom: 60px;
            }    
        }   
        .bundled-items { 
            overflow: hidden;
            #crust-items {
                @media (min-width: 992px) {
                    float: left;
                    width: 75%;
                    @include padding-end(15px);
                }
            }
            .action-bundle {
                @media (min-width: 992px) {
                    float: left;
                    width: 25%;
                    @include padding-start(15px);
                }
                @media (max-width: 991px) and (min-width: 576px) {
                    float: right;
                    max-width: 200px;
                }
                @media (max-width: 575px) {
                    margin-top: 20px;
                }
                .total-price {
                    margin: 0 0 15px;
                    .price {
                        color: $body-price-sale-color;  
                        font-size: $size-h3 + 4px;
                    }
                }
                .btn { 
                    padding: 10px 25px;    
                    font-size: $button-size + 4px; 
                    @media (max-width: 991px) {
                        padding: 10px 15px;  
                        font-size: $button-size - 1px;  
                    }
                    @media (max-width: 575px) {
                        width: 100%;
                        display: block; 
                    } 
                }
            }
            .bundled-images {
                padding: 0;
                list-style-type: none;
                overflow: hidden;
                li {
                    position: relative;
                    padding: 20px 25px;
                    overflow: visible;
                    float: left;
                    list-style: none;
                    text-align: center;
                    &:not(:first-child) {
                        &:after {
                            content: '';
                            height: 70%;
                            width: 1px;
                            background: $body-border-color;
                            @include ab(absolute, block, 50%, 0, n, n);
                            @include translate(0, -50%);
                        }
                        &:before {
                            content: '+';
                            height: 32px;
                            width: 32px;
                            line-height: 30px;
                            text-align: center;
                            border: 1px solid $body-border-color;
                            background: $body-background-color;
                            color: $body-general-color;
                            font-size: $size-h4;
                            z-index: 2;
                            @include ab(absolute, block, 50%, -16px, n, n);
                            @include translate(0, -50%);
                            @include border-radius-circle;
                        }
                    }
                    @media (min-width: 576px) {            
                        width: 33.333333%;
                    }  
                    @media (max-width: 575px) {             
                        width: 50%;
                        @include padding-start(20px);
                        @include padding-end(20px);
                        &:nth-child(3) {
                            clear: both;  
                            width: 100%;
                            padding-top: 30px;
                            &:after {
                                height: 1px;
                                width: 200%;
                                @include ab(n, n, 0, -25%, n, -25%);               
                                @include translate(0, 0);
                            }
                            &:before {
                                @include ab(n, n, -16px, 50%, n, auto);  
                                @include translate(-50%, 0);            
                            }
                        }
                    }
                    h5 {
                        color: $body-link-color;     
                        font-size: $product-font-size;      
                        font-weight: $font-bold;
                        line-height: 18px; 
                        overflow: hidden; 
                        margin: 0 0 15px; 
                        display: -webkit-box;
                        display: -ms-box;
                        -webkit-line-clamp: 2;
                        -webkit-box-orient: vertical;
                        @media (max-width: 479px) {
                            font-size: $product-font-size - 2px;      
                        }    
                        a {
                            color: $body-link-color;
                            &:hover {
                                color: $body-link-color-hover;
                                text-decoration: underline; 
                            }                              
                        } 
                    }
                    img { 
                        display: block; 
                        margin: 0 auto 10px;
                    }
                    .price {
                        display: block;
                    }
                    &:first-child .price {
                        color: $body-general-color;
                    } 
                }
            }    
            .wrap-bundled-item {
                overflow: hidden;
                @media (max-width: 991px) and (min-width: 576px) { 
                    width: calc(100% - 200px); 
                    float: left;
                }
            }
        }
    }
    .bundled-item {
        position: relative;
        line-height: 20px;
        float: left;
        margin: 0 0 7px;
        padding: 0;
        @include padding-start(30px);     
        input[type="checkbox"] {
            @include ab(absolute, block, 0, 0, n, n);
            @include opacity(0);
            margin: 0;
            width: 100%;
            height: 100%;     
            z-index: 2;     
        }   
        .wrap-item,
        > .b-info {
            position: relative;
            &:before {
                content: '';
                width: 20px;
                height: 20px;
                border: none;
                background: #000000;
                @include ab(absolute, block, 50%, -30px, n, n);
                @include border-radius(3px, 3px, 3px, 3px);
                @include translate(0, -50%);
            }
        } 
        input[disabled] {
            + .b-info:before, + .wrap-item:before {
                background: #e6e6e6;   
            }
            &:checked {
                + .b-info:after, + .wrap-item:after {
                    content: none;
                }  
            } 
        }
        input:checked {
            + .b-info:after, + .wrap-item:after { 
                content: '\e83d';
                font-family: $font-family-icon;  
                font-size: 12px;
                line-height: 12px;
                color: #ffffff; 
                @include ab(absolute, block, 50%, -26px, n, n);  
                @include translate(0, -50%);
            }      
        }       
        .bundled-price {
            display: inline;
            @include padding-start(5px);
            .price {
                font-size: $body-font-size + 2px;
                color: $body-price-sale-color;
            } 
        }
        .bundled-product-title {
            color: $body-link-color;
            font-size: $body-font-size - 1px;  
        }
        &:not(:first-child) {
            clear: both;
            cursor: pointer;
            input[type="checkbox"] {
                cursor: pointer;
            }  
        }
    } 
