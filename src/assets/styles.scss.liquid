} 
.product-simple-tab {
    padding: 0 0 72px;
    overflow: hidden;
    position: relative;  
    clear: both;
    @media (max-width: 991px) {
        padding: 0 0 40px;    
    }   
    @media (max-width: 767px) {
        padding: 0 0 34px;   
    } 
