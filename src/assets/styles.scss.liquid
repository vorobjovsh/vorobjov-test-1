        padding: 0 0 15px;    
    }  
    .group-toolbar {
        align-items: center;
        @include flexbox;
        @media (max-width: 991px) {
            flex-wrap: wrap;
        } 
    }    
    .cata-title {
        @include margin-end(auto);
        @media (max-width: 991px) {
            width: 100%;
            padding-bottom: 10px; 
        }  
        h1 {
            text-transform: uppercase; 
            @media (max-width: 991px) {
                font-size: $size-h1 - 10px;
            }
        }
    } 
    .filter-icon-order {
        float: right;
        line-height: 38px;
        cursor: pointer;
        padding: 0;
        @include padding-end(4%);
        i { 
            float: left;
            font-size: $font-small-icon;
            color: $body-general-color;
            @include margin-end(7px);
            @include transition(all 0.35s ease 0s);
            &:before {
                display: block;
                line-height: 38px;
            }
        }
        &:hover i {
            color: $body-link-color-hover;
        }   
    }  
    .grid-list {          
        .text {
            float: left;
            display: none;  
            margin: 0; 
            @include margin-end(10px);         
        }   
        .grid {
            margin: 0 5px;     
            float: left;
            text-align: center;
            border: none;     
            cursor: pointer; 
            font-size: 0;  
            background: none;
            color: $text-other;       
            @include transition(all 0.35s ease 0s); 
            &.grid-5 {
                @media (max-width: 1199px) {
                    display: none; 
                }
            }     
            &.grid-4 {
                @media (max-width: 991px) {
                    display: none; 
                }
            }
            &.grid-3 {  
                @media (max-width: 767px) {
                    display: none; 
                }  
            }
            i {
                font-size: 16px;  
                display: block;
            }
            &:first-child {
                @include margin-start(0);
            }
            &.active, &:hover {
                background: none;
                color: $body-general-color;        
            } 
        }    
    }
    .filter {
        padding-bottom: 20px;
        margin-bottom: -1px;
        @include margin-end(40px);
        &:hover > .dropdown-menu {
            display: block;
            z-index: 11;
        } 
        > a {
            border: 1px solid $body-border-color;
            padding: 9px 20px;
            font-size: 14px;
            display: inline-block;
            width: 140px;
            text-align: center;
            i {
                float: right;
                margin-top: 4px;
            }
            &:hover { 
                text-decoration: none;
                color: $body-link-color;
            }
        }
        > .dropdown-menu {   
            @include ab(n, n, n, 15px, n, auto); 
            width: 100%;
            padding: 25px 30px;     
        }
        .clear-filter {
            display: none;
        } 
    }  
    .sort-by {
        color: $body-general-color;
        @include margin-start(53px);
        @media (max-width: 767px) {
            @include margin-start(15px);
        }  
        @media (max-width: 575px) {
            @include margin-start(0);  
        }     
        .sort-by-inner {
            position: relative;
            text-transform: uppercase;
            font-size: $body-font-size - 2px; 
            cursor: pointer;  
            width: fit-content;
            align-items: center;  
            @include flexbox;
            label {      
                margin: 0;          
                color: $body-general-color; 
            }    
            #cata_sort_by {
                display: inline-block;
                vertical-align: middle;
                position: static;
                &:after {
                    content: none;
                } 
                button {
                    align-items: center;
                    border: none;
                    padding: 0;
                    background: none;
                    line-height: 1.3;
                    text-align: start;   
                    font-size: $body-font-size - 2px;
                    font-weight: $font-bold;  
                    text-transform: uppercase;  
                    @include flexbox;      
                    a {
                        color: $body-link-color; 
                        text-decoration: underline;  
                    }   
                    span {
                        padding: 0 10px; 
                        color: $body-link-color;                  
                        @media (max-width: 479px) {
                            @include padding-end(6px);
                        }
                    }
                    i {        
                        text-align: end;
                        padding: 0;
                        content: none;      
                        color: $body-link-color; 
                        &:before {
                            font-size: 22px;
                        }               
                    }
                }
            }         
        }
        #sort_by_box {      
            width: auto;
            min-width: 175px;      
            background: $white;           
            padding: 0;      
            list-style: none;
            border: 1px solid $body-border-color;
            @include ab(absolute, block, 100%, n, n, -1px);
            @include dropdown-hidden;
            @include box-shadow(0 6px 12px rgba($black, 0.176));
            @include translate(0, 10px);
            @include transition(transform 0.25s ease-in-out);
            @media (max-width: 767px) {
                @include ab(n, n, n, auto, n, 0);  
            }    
            li {
                padding: 0 10px;
                text-align: start;
                a {
                    padding: 4px 0;    
                    line-height: 20px;      
                    display: block;
                    transition: none;                               
                    font-size: $body-font-size - 2px;   
                    color: $body-link-color;
                }
                &:hover { 
                    @media (min-width: 1025px) {
                        background:$body-background-color;
                        a {
                            color: $body-link-color-hover;            
                        }
                    }          
                }
                &.active {          
                    background: $body-background-color;
                    a {
                        cursor: pointer;
                        color: $body-link-color-hover;            
                    }
                }        
            }
        }
        &.opened #sort_by_box {
            @include dropdown-visible;
            z-index: 1001;
            @include translate(0, 0);  
        }  
    }
    .show-by {
        position: relative;
        display: inline-block;
        margin: 0 0 10px;
        @include margin-start(4%);
        .show-by-button {
            border: none;
            padding: 6px 20px;
            line-height: 38px;
            background: $body-background-color;  
        }
        #show-by-box {
            width: auto;     
            min-width: 85px;
            background: $white;          
            padding: 10px 20px; 
            visibility: hidden;
            z-index: -99;
            list-style: none;
            text-align: start;
            @include ab(absolute, block, 100%, 0, n, 0);
            @include opacity(0); 
            @include translate(0, 10px); 
            @include  transition(all 0.35s ease);
            li {
                padding: 0;        
                margin-bottom: 0; 
                border-bottom: 1px solid $body-border-color;
                a {
                    padding: 8px 0;
                    line-height: 26px;
                    display: block;                             
                    font-size: $body-font-size;
                    color: $body-general-color;
                    @include transition(none);
                }
                &:hover {     
                    @media (min-width: 1025px) {
                        a {
                            color: $main-color;            
                        }
                    }         
                }
                &.active { 
                    > span {
                        padding: 8px 0;
                        line-height: 26px;
                        display: block;           
                        font-size: $body-font-size;
                        color: $main-color; 
                    }         
                    a {
                        cursor: default;
                        color: $main-color;           
                    }
                }
                &:last-child {
                    border-bottom: none;          
                }
            }
        }
        &:hover,
        &.opened {
            #show-by-box {
                visibility: visible;
                z-index: 99;          
                @include opacity(1);  
                @include translate(0, 0);
            }
        }
    }
