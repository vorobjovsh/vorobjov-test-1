/*=== comments ===*/
.comment-form {  
    h3 {                
        margin: 0 0 6px;    
        position: relative;
        font-size: $size-h1;
        text-transform: uppercase; 
    }  
    p {
        margin: 0 0 13px; 
    }    
    .control-wrapper {
        input {
            width: 100%;     
            height: 38px;         
            margin-bottom: 25px;  
            padding-top: 8px;    
            padding-bottom: 8px; 
        }
        textarea {
            width: 100%; 
            max-width: 100%;      
            min-height: 180px;
            margin: 0; 
        }    
        &:last-child {
            padding-top: 0;
        }    
    }
    > .btn {
        margin: 25px 0 0;
        padding: 5px 30px;
    }
}
.grid-comments {
    h5 {
        margin: 0 0 15px;    
        position: relative;
        font-size: $size-h1;
        text-transform: uppercase;  
    }
    .comments {
        list-style: none;
        margin: 45px -26%;
        padding: 44px 0 0;
        border-top: 1px solid $body-border-color; 
        @media (max-width: 1024px) {
            margin: 45px -10.5%; 
            padding: 34px 0 0;
        }
        @media (max-width: 767px) {
            margin: 25px 0;
            padding: 25px 0 0; 
        } 
        .comment {
            border-bottom: 1px solid $body-border-color;
            margin: 0 0 44px;
            padding: 0 0 44px;
            @media (max-width: 1024px) {
                margin-bottom: 34px;
                padding-bottom: 34px; 
            } 
            @media (max-width: 767px) {
                margin-bottom: 25px;
                padding-bottom: 25px;
            }
            &:last-child {
                margin-bottom: 0; 
            }  
        }  
        .load-comment {
            display: table;
            width: 100%;
            color: $body-general-color; 
            .comment-avatar {
                display: table-cell;
                vertical-align: top;
                width: 80px; 
                @include border-radius-circle;
                @media (max-width: 479px) {
                    width: 50px; 
                } 
                img {
                    width: 100%;
                    @include border-radius-circle;
                }
            } 
            .author {
                font-weight: $font-bold;
                font-size: $size-h4;
                text-transform: uppercase;
            }
            .date {
                font-weight: $font-bold; 
                font-size: $body-font-size - 3px;
            } 
            p { 
                margin: 10px 0 0;
                font-size: $body-font-size; 
            }
        }
    }
}
/*=== popup-sell-people ===*/
.popup-sell-people {
    width: 410px; 
    vertical-align: middle;
    position: fixed;
    display: none; 
    background: $body-background-color;
    border: 1px solid #cccccc;   
    z-index: 1002;
    padding: 20px;
    @include box-shadow(0px 2px 5px rgba(0, 0, 0, 0.1));
    @media (max-width: 575px) { 
        width: calc(100% - 20px);    	
    }
    &.show {
        display: block;
    }
    &.bottom-left {
        @include ab(n, n, n, 10px, 10px, n);
        @media (max-width: 991px) {
            bottom: 64px; 
        } 
    }
    &.bottom-right {
        @include ab(n, n, n, n, 10px, 10px);
        @media (max-width: 991px) {
            bottom: 64px; 
        } 
    }
    &.top-left {
        @include ab(n, n, 10px, 10px, n, n);
    }
    &.top-right {
        @include ab(n, n, 10px, n, n, 10px);
    }
    .img-user {
        width: 40px;		
        display: table-cell;
        vertical-align: middle;
        img {
            width: 100%;
        }
    }	
    .text-detail {
        display: table-cell;
        vertical-align: middle; 
        @include padding-start(22px);     
        .sell-text {
            margin: 0;
            line-height: 20px;
            font-size: $body-font-size;
            font-weight: 400;
            strong {
                display: block;
                font-size: 22px;
                margin-bottom: 5px;  
            }
        }
    }
}
/*=== discount-modal ===*/
#discount-modal {
    z-index: 99991; 
    @include ab(fixed, none, 0, 0, n, n); 
    width: 100%;
    height: 100%;
    margin: 0 auto;
    .underlay {
        background: rgba(0, 0, 0, 0.5);   
        height: 100%;
        @include ab(fixed, n, 0, 0, n, n);
        width: 100%;
        z-index: 99992;    
    }
    .wrap-modal {
        background: $body-background-color;
        z-index: 99999;
        padding: 0;        
        margin: 0 calc((100% - 750px) / 2); 
        width: 750px;
        @include ab(absolute, block, 50%, 0, n, n);
        @include translate(0, -50.1%);      
        @media (max-width: 767px) {       
            width: 90%;
            margin: 0 5%;
            max-height: 90%;    
        }
        > a {
            i:before {
                @include close-btn; 
            }         
            &:hover {
                text-decoration: none; 
            } 
        }
    }
    .modal-body {
        padding: 0;
        min-height: 405px;  
        align-items: center;
        justify-content: space-between; 
        @include flexbox;
        @media (max-width: 767px) { 
            max-height: 100%;
            overflow-y: auto;
        }
        .image {
            min-width: 328px;
            @include padding-start(15px);
            @media (max-width: 767px) {
                display: none;
            }  
        }
        .content { 
            padding: 15px;
            @include padding-start(60px); 
            @media (max-width: 767px) {
                padding: 25px 30px;
                width: 100%; 
            }
            h5 {
                font-weight: 600; 
                font-size: 60px;
                line-height: 1.0;
                @media (max-width: 767px) {
                    font-size: 35px;
                }
            }
            p {
                line-height: 1.1;
                margin: 16px 0 39px; 
                font-size: 30px;
                font-weight: 300;
                @media (max-width: 767px) {
                    font-size: 20px; 
                } 
            }
            .btn {
                text-transform: uppercase;
                font-size: 16px; 
                padding: 20px 40px;
                display: block;
                @media (max-width: 767px) {
                    padding: 15px; 
                }         
            }
        }
    }
} 

