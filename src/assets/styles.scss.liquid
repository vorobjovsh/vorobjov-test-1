            i, svg { 
                float: left; 
                font-size: $font-small-icon - 6px;  
                margin: 0;
                @include margin-end(12px);                 
            }
            &:hover { 
                color: $body-link-color-hover;   
                svg {
                    fill: $body-link-color-hover !important; 
                }  
                &.icon-9 svg { 
                    stroke: $body-link-color-hover !important;
                }   
            }       
        }  
        .add-to-wishlist {
            @include margin-end(30px); 
        }
        #widget-fave-html {
            .ssw-fave-btn {
                display: inherit;
                line-height: 1.3; 
            } 
            .ssw-fave-btn-content {
                border: none;
                @include padding-start(0);
                @include padding-end(0);  
                i {
                    color: inherit; 
                    &:before {
                        margin: 0;
                    }
                }
                &:active, &:focus { 
                    outline: none;
                    @include box-shadow(none); 
                }  
            } 
            .ssw-fave-btn-counter {
                display: none;
            }
        }
        .product-action {     
            .variants-wrapper {       
                .selector-wrapper {
                    margin-bottom: 26px;   
                    clear: both;
                    text-align: start;      
                    display: none;  
                    @media (max-width: 767px) {
                        margin-bottom: 15px; 
                    }      
                    select { 
                        min-width: 50%;
                        height: 34px;
                        padding: 0 10px;
                        line-height: 32px;            
                        font-size: $body-font-size;        
                        color: $body-general-color;                                                     
                    }          
                }
                label {    
                    @include header-swatch;
                    width: 110px;     
                }  
            } 
            .wrap-quantity-add-cart {
                list-style: none; 
                padding: 0;
                margin: 0;      
            }     
            .quantity-product {      
