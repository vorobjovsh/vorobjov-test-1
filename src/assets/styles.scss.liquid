    #product-image {   
        margin: 0 0 30px;                    
    }
    .product-image-inner {
        margin: 0;
        padding: 0;
        position: relative;
        .slick-slide {
            display: block;
            float: left;    
            z-index: 0 !important;
            &.slick-active {
                z-index: 99 !important;  
            }       
        } 
        .slider-main-image {
            width: calc(100% - 70px);
            float: right;
            position: relative; 
            @media (max-width: 991px) {
                width: 100%;
            }
            .slick-list.draggable {
                position: relative;
                overflow: hidden; 
            }
            .slider-for-07 .slick-track {
                display: block;
            }   
            .slick-track {
                align-items: center;
                @include flexbox;    
            }     
            .slick-item:not(:first-child) {
                @include opacity(0); 
                visibility: hidden;
                height: 0;
                @include transition(opacity 0.35s ease-in-out);
            }  
            .slick-current .slick-item { 
                @include opacity(1);
                visibility: visible;
                height: auto;    
            }  
            .slick-arrow {
                @include ab(absolute, block, 50%, n, n, n);
                width: 60px;
                height: 60px;
                line-height: 60px;
                background: $body-background-color;
                color: $body-general-color;
                z-index: 1000;     
                text-align: center; 
                cursor: pointer;
                @include opacity(0);
                visibility: hidden;
                @include translate(0, -50%);     
                @media (max-width: 1024px) {
                    @include opacity(1);
                    visibility: visible;
                    width: 40px;
                    height: 40px;
                    line-height: 40px;  
                }
                i { 
                    font-size: $font-small-icon - 2px;  
                    display: block; 
                } 
                &.btn-prev {
                    @include ab(n, n, n, 30px, n, n); 
                    @media (max-width: 1024px) {
                        @include ab(n, n, n, 0, n, n);
                    }
                    i:before {
                        @include prev-icon;
                    }
                }  
                &.btn-next {
                    @include ab(n, n, n, n, n, 30px); 
                    @media (max-width: 1024px) {
                        @include ab(n, n, n, n, n, 0); 
                    } 
                    i:before {
                        @include next-icon;  
                    } 
                }
                &:hover { 
                    color: $main-color;  
                } 
            } 
            &:hover {
                .slick-arrow {
                    @media (min-width: 1025px) {
                        @include opacity(1);
                        visibility: visible;
                    }  
                } 
            }
        } 
        .slick-track.no-translate3d {
            @include transform(translate3d(0, 0, 0) !important); 
        }  
        .slider-thumbs-01 {
            width: 60px; 
            float: left;
            @media (max-width: 991px) {
                width: 100%;
                min-width: 60px;  
            }
            .slick-list.draggable {
                overflow: hidden;
                @media (max-width: 991px) {
                    margin: 0 -5px;
                }  
            } 
            .slick-item {  
                @include opacity(0);
                visibility: hidden;
                height: 0;
                @include transition(opacity 0.35s ease-in-out);
            } 
            .slick-slide {
                padding-bottom: 10px;
                display: inline-block;   
                @media (max-width: 991px) {
                    padding: 10px 5px 0;    
                }
                .slick-item { 
                    cursor: pointer;   
                    @include opacity(1);
                    visibility: visible;
                    height: auto; 
                }  
                &.slick-current .slick-item {
                    border: 2px solid $main-color;
                }    
            } 
        } 
        .meta-image360 {
            @include ab(absolute, block, 15px, n, n, 10px); 
            z-index: 1000;
            > a {
                padding: 6px 15px;
                line-height: 20px;
                border: 1px solid $main-color;
                display: inline-block;
                color: $main-color;        
                .icon-360 {
                    width: 30px;
                    height: 19px;
                    float: left;
                    position: relative;
                    top: 2px; 
                    background-image: url({{'icon-360.png' | asset_url }});
                    background-size: cover;
                    background-position: center center;
                    background-repeat: no-repeat;
                    margin: 0; 
                    @include margin-end(5px);
                }
            }
        }
        .meta-video {
            z-index: 99;    
            @include ab(absolute, block, 10px, n, n, 10px);
            .m-video {        
                border: 1px solid $body-border-color;
                background: #ffffff;    
                color: $body-general-color;
                line-height: 30px;
                text-align: center;        
                display: block;
                padding: 2px 15px;        
                i {
                    @include margin-end(5px);
                }
                &:hover {
                    color: $main-color;
                }
            }
            .fancybox-media {
                display: block;
                line-height: 24px;
                padding: 6px 20px;
                border: 1px solid $main-color;
                background: none;
                color: $main-color;    
                .fa {
                    margin: 0; 
                    @include margin-end(7px);
                }
                &:hover {
                    @media (min-width: 1025px) {
                        color: $white;  
                        background: $main-color;
                        border-color: $main-color
                    }
                }
            }
        }
        .product-single__media {
            position: relative;
            width: 100%;
            height: 0;
            padding-bottom: 100%;
            iframe, model-viewer {
                position: absolute;
                width: 100%;
                height: 100%;  
            } 
        }   
    }  
