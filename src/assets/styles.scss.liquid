        text-align: center;
        font-weight: $font-bold;
        font-size: $size-h1;
        font-family: $font-heading;
        padding: 30px 0 10px;
    }  
    .nav { 
        padding-bottom: 22px; 
        margin-bottom: 0;        
        position: relative;             
        @media (max-width: 991px) {
            padding-bottom: 15px;         
        }      
        @media (max-width: 812px) {
            -webkit-overflow-scrolling: touch;
        }        
        li a {    
            font-size: $size-h1;
            text-transform: uppercase; 
            @media (max-width: 1440px) {
                font-size: $size-h1 - 6px;
            } 
            @media (max-width: 991px) {
                font-size: $size-h1 - 10px; 
            }                 
            &:before {
                content: "";
                @include ab(absolute, block, n, 0, -5px, 0);  
                width: 100%;
                height: 3px;
                background: $body-link-color-hover;
                @include opacity(0);  
                visibility: hidden;
                @include transition(opacity 0.35s ease-in-out);
                @media (max-width: 575px) {
                    bottom: 0;
                }   
            }  
            &.show,    
            &.active { 
                &:before {
                    @include opacity(1);
                    visibility: visible;
                }        
            }  
        }
    }  
    .product-description {
        h1, h2, h3, h4, h5, h6, p, div {
            margin: 0 auto; 
            padding-bottom: 26px; 
            @media(max-width: 767px) {
                padding-bottom: 15px; 
            }   
