    .info {
        padding: 15px 20px;
    }
    .custom-block-heading-text {
        margin: 0 auto 3%;
        &.big-size {
            @media (max-width: 991px) {
                font-size: 25px !important;
            }
        }
    }
    .custom-block-body-text.big-size {
        @media (max-width: 991px) {
            font-size: 20px !important;
        }
    }
    .btn-banner {
        text-transform: capitalize;
    }
}
.custom-item {
    margin: 0 0 30px;
}
.custom-item-inner {
    position: relative;
    max-width: 100%;
    &.custom-item-inner-bgimage {
        max-width: 1170px;
        margin: 0 auto;
        padding: 0 4%;
        .custom-block-heading-text {
            max-width: 100%;
        }
