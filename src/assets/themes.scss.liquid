                @include translate(0, -50%);
                z-index: 99;
                margin: 0;
                width: 560px;
            }
            .text {
                padding: 15px 0 0;
            }
            .product-grid-carousel-v3 {
                margin: 0;
                @media (min-width: 768px) {
                    padding: 0 15px;
                }
                .product-grid-item {
                    padding: 30px 12px 25px;
                    position: relative;
                    &:before {
                        content: "";
                        @include ab(absolute, block, 0, -15px, n, -15px);
                        height: 1px;
                        background: $body-border-color;
                        @media (max-width: 767px) {
                            @include ab(n, n, n, 0, n, 0);
                        }
                    }
                    @media (min-width: 768px) {
                        &:nth-child(-n+3) {
                            &:before {
                                content: none;
                            }
                        }
                    }
                    @media (max-width: 767px) {
                        &:nth-child(-n+2) {
                            &:before {
                                content: none;
                            }
                        }
                    }
                }
                .product-wrapper {
                    margin-top: 0;
                    .product-head .detail-deal {
                        display: none;
                    }
                    .product-content {
                        padding: 0;
                    }
                }
