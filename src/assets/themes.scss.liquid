}
.custom-item-inner-product .image .padding {
    margin-bottom: 15px;
    display: block;
}
.custom-item-inner-product,
.custom-item-inner-collection {
    background: #ebebeb;
    .info {
        > a {
            margin: 0;
            line-height: 1.1;
            color: $body-link-color;
            font-size: $size-h5;
            text-transform: uppercase;
            font-family: $font-heading;
            @media (max-width: 479px) {
                @include transform-text;
            }
        }
        .product-author {
            color: $body-general-color;
            font-size: $body-font-size;
            padding: 0 0 10px;
        }
        .product-name {
            color: $body-link-color;
            margin: 0 0 5px;
            @include transform-text;
        }
        .product-price {
            display: inline-block;
        }
    }
}
/*=== Section menu list ===*/
.home-menu-list {
    .menu-list-box {
        padding: 15px 0;
    }
    .border-top {
        border-top: 1px solid $body-border-color !important;
    }
    .border-bottom {
        border-bottom:  1px solid $body-border-color !important;
    }
    .heading {
        float: left;
        @include padding-end(40px);
        text-transform: uppercase;
        color: $main-color;
        @media (max-width: 767px) {
            @include padding-end(15px);
        }
    }
    .text-center > ul {
        justify-content: center;
    }
    ul {
        flex-wrap: wrap;
        li {
            a {
                padding: 0 15px;
                color: $body-link-color;
                display: block;
                font-size: $body-font-size + 1px;
                @media (max-width: 767px) {
                    padding: 0 7px;
                }
                &:hover {
                    color: $body-link-color-hover;
                }
            }
            &:first-child a {
                @include padding-start(0);
            }
        }
    }
}
/*===  Section recently-viewed ===*/
.section-recently-viewed {
    padding-top: 34px;
    padding-bottom: 48px;
    @media (max-width: 767px) {
        padding-top: 20px;
        padding-bottom: 20px;
    }
    .style-1 {
        .product {
            @include flexbox;
            flex-wrap: wrap;
            align-items: center;
            margin-top: 40px;
            &:nth-child(-n+4) {
                @media (min-width: 992px) {
                    margin-top: 0;
                }
            }
            &:nth-child(-n+3) {
                @media (max-width: 991px) and (min-width: 768px) {
                    margin-top: 0;
                }
            }
            &:nth-child(-n+2) {
                @media (max-width: 767px) and (min-width: 576px) {
                    margin-top: 0;
                }
            }
            @media (max-width: 575px) {
                margin-top: 20px;
                &:first-child {
                    margin-top: 0;
                }
            }
            .product-image {
                width: 80px;
            }
            .product-content {
                @include padding-start(20px);
                display: block;
                width: calc(100% - 80px);
                a {
                    color: $body-link-color;
                    display: block;
                    padding-bottom: 6px;
                    &:hover {
                        color: $body-link-color-hover;
                    }
                }
            }
        }
    }
    .style-2 {
        .collection {
            justify-content: center;
        }
        .product {
            max-width: 11.11111%;
            @include flex(0 0 11.11111%);
            @media (min-width: 992px) {
                &:nth-child(-n+9) {
                    margin-top: 0;
                }
            }
            @media (max-width: 991px) and (min-width: 768px) {
                max-width: 16.66667%;
                @include flex(0 0 16.66667%);
                &:nth-child(-n+6) {
                    margin-top: 0;
                }
            }
            @media (max-width: 767px) and (min-width: 576px) {
                max-width: 33.33333%;
                @include flex(0 0 33.33333%);
                &:nth-child(-n+3) {
                    margin-top: 0;
                }
            }
            @media (max-width: 575px) {
                max-width: 50%;
                @include flex(0 0 50%);
                &:nth-child(-n+2) {
                    margin-top: 0;
                }
            }
            .product-content {
                display: none;
            }
        }
    }
}
/*=== Section Shop by category ===*/
.shop-by-categgory {
    .shop-by-categgory-container {
        position: relative;
    }
    .block-content {
        @include ab(absolute, n, 30px, 0, n, 0);
        @include flexbox;
        z-index: 99;
        flex-direction: column;
        height: calc(100% - 100px);
        @media (max-width: 991px) {
            position: static;
        }
    }
    .b-heading {
        font-size: $size-h1 + 10px;
        font-weight: $font-bold;
        text-align: center;
        margin-bottom: 20px;
        @media (max-width: 991px) {
            font-size: $size-h1 + 5px;
            margin-top: -64px;
            z-index: 99;
        }
        @media (max-width: 767px) {
            font-size: $size-h1;
        }
    }
    .b-textbox {
        margin-top: auto ;
        @include margin-start(115px);
        @media (max-width: 1199px) {
            display: none;
        }
        .b-subheading {
            font-size: $body-font-size + 10px;
            font-weight: 300;
            margin-bottom: 4px;
            @media (max-width: 1440px) {
                font-size: $body-font-size + 5px;
            }
        }
        .b-caption {
            font-size: $size-h1 + 10px;
            font-weight: $font-bold;
            line-height: 1.2;
            margin-bottom: 22px;
            @media (max-width: 1440px) {
                font-size: $size-h1;
            }
        }
        .b-btn {
            @media (max-width: 1440px) {
                display: none;
            }
        }
    }
    .block-collection-icon {
        @include flexbox;
        align-items: center;
        @media (max-width: 991px) {
            display: block;
        }
        > div {
            width: 50%;
            @media (max-width: 991px) {
                width: 100%;
                padding-top: 20px;
            }
        }
        .item-col-list {
            @include padding-end(50px);
            @media (max-width: 1440px) {
                @include padding-end(25px);
            }
            @media (max-width: 1199px) {
                @include padding-end(10px);
            }
            @media (max-width: 991px) {
                @include padding-end(0);
            }
        }
        .item-col-icon {
            @include padding-start(50px);
            @media (max-width: 1440px) {
                @include padding-start(25px);
            }
            @media (max-width: 1199px) {
                @include padding-start(10px);
            }
            @media (max-width: 991px) {
                @include padding-start(0);
            }
        }
        .block-or {
            width: auto;
            font-size: 45px;
            font-weight: $font-bold;
            @include box-shadow(none);
            @media (max-width: 1199px) {
                display: none;
            }
        }
    }
    .item-list {
        padding: 25px 40px 30px;
        background-repeat: no-repeat;
        background-size: cover;
        background-position: right center;
        @include box-shadow(0px 0px 15px rgba(0, 0, 0, 0.3));
        .title-wrapper {
            margin-bottom: 20px;
        }
        ul li {
            padding-bottom: 5px;
            a {
                font-size: $body-font-size - 1px;
            }
        }
    }
    .icon-list-inner {
        background: #ffffff;
        padding-top: 25px;
        @include box-shadow(0px 0px 15px rgba(0, 0, 0, 0.3));
        .title-wrapper {
            padding: 0 40px;
            margin-bottom: 20px;
        }
        ul {
            @include flexbox;
            flex-wrap: wrap;
            li {
                max-width: 25%;
                @include flex(0 0 25%);
                text-align: center;
                padding: 28px 15px;
                border-top: 1px solid $body-border-color;
                @include border-end(1px solid $body-border-color);
                &:nth-child(4n) {
                    @include border-end(none);
                }
                span {
                    display: block;
                    padding-top: 10px;
                    font-weight: $font-bold;
                }
            }
        }
    }
}
/*=== Fix skin Fashion-simple ===*/
.skin-simple .product-grid-section-v5 .menu-column {
    > ul li a {
        font-size: $body-font-size - 2px;
        text-transform: uppercase;
