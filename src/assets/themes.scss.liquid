        background-size: 100%;
    }
    &.layout-right-column .separator-content {
        @media (min-width: 992px) {
            width: 83.33333%;
        }
    }
}
.separator-heading {
    max-width: 1200px;
    padding: 0 15px;
    margin: 0 auto;
}
.separator-line {
    border-top-width: 1px;
    border-bottom-width: 0;
    border-left-width: 0;
    border-right-width: 0;
    max-width: 1170px;
    margin: 0 auto;
    &.line-solid {
        border-style: solid;
    }
    &.line-dotted {
        border-style: dotted;
    }
    &.line-dashed {
        border-style: dashed;
