}
.product-grid-border {
    /*==== border-bottom ===*/
    .product-grid-item {
        padding-bottom: 30px;
        position:  relative;
        &:after {
            content: "";
            @include ab(absolute, block, n, 0, 0, n);
            width: 100%;
            height: 1px;
            background: $body-border-color;
        }
    }
    .product-grid-carousel {
        position: relative;
        &:after {
            content: "";
            @include ab(absolute, block, n, 0, 0, n);
            width: 100%;
            height: 1px;
            background: $body-border-color;
            z-index: -1;
        }
        .product-grid-item {
            &:before {
                @include ab(absolute, block, n, -5px, n, n);
            }
            &:after {
                content: none;
            }
        }
    }
