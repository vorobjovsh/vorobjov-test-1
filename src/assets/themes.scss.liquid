                .countdown-amount {
                    @media (min-width: 1440px) {
                        font-size: $body-font-size;
                    }
                }
                .countdown-period {
                    @media (min-width: 1440px) {
                        font-size: 7px;
                    }
                }
            }
        }
        &.bg-color {
            padding: 25px 30px;
            .owl-carousel .owl-nav button {
                &.owl-prev {
                    @include ab(n, n, n, -63px, n, n);
                    @media (max-width: 1440px) {
                        @include ab(n, n, n, -33px, n, n);
                    }
                }
                &.owl-next {
                    @include ab(n, n, n, n, n, -63px);
                    @media (max-width: 1440px) {
                        @include ab(n, n, n, n, n, -33px);
                    }
                }
            }
        }
