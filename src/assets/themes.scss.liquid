                .countdown-period {
                    @media (min-width: 1440px) {
                        font-size: 7px;
                    }
                }
            }
            .nav-tabs {
                margin: 0 !important;
                border-bottom:  1px solid $body-border-color;
                @include flexbox;
                flex-wrap: nowrap;
                li {
                    padding: 0 !important;
                    flex-grow: 1;
                    &:last-child {
                        @include margin-end(auto);
                        a {
                            @include border-end(none);
                        }
                    }
                    &:first-child {
                        @include margin-start(auto);
                    }
                    a {
                        padding: 18px 15px;
                        @include border-end(1px solid $body-border-color);
                        color: $body-link-color;
                        @include opacity(0.8);
                        &:before {
                            content: "";
                            @include ab(absolute, block, n, 0, 0, 0);
                            height: 3px;
                            z-index: 1;
                            @include opacity(0);
                        }
                        img {
                            margin: 0 auto 10px;
                            max-width: 35px;
                            display: block;
                        }
                        &.active, &:hover {
                            @include opacity(1);
                            color: $body-link-color;
                            &:before {
                                @include opacity(1);
                            }
                        }
                    }
                }
            }
            .tabs-list-carousel .box-product {
                margin: 0;
                padding: 30px 40px 33px;
                @media (max-width: 767px) {
                    padding: 20px 20px 23px;
                }
            }
            .owl-carousel .owl-nav button {
                &.owl-prev {
                    @include ab(n, n, n, 0, n, n);
                }
                &.owl-next {
                    @include ab(n, n, n, n, n, 0);
                }
            }
        }
    }
    .tab-content {
        position: relative;
        @include clearfix();
        > .tab-pane {
            visibility: hidden;
            width: 100%;
            @include opacity(0);
            @include ab(absolute, block, 0, 0, n, n);
            @include transition(all 0.45s cubic-bezier(0.25, 0.46, 0.45, 0.94));
            &.active {
                @include opacity(1);
                position: static;
                visibility: visible;
            }
        }
    }
    .fix-background {
        background: $body-background-color;
        padding: 51px 120px 44px;
        @media (max-width: 1199px) {
            @include padding-start(60px);
            @include padding-end(60px);
        }
        @media (max-width: 767px) {
            padding: 23px 30px 20px;
        }
        .hp-tabs-list.tabs-list-row {
            &:before, &:after {
                background: $body-background-color !important;
            }
        }
    }
}
.hp-tabs-list {
    clear: both;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
    &.tabs-list-carousel .box-product {
        @include carousel-loading();
    }
    &.tabs-list-row .box-product {
        overflow: hidden;       //fixed left right border-top & border-bottom
    }
    .text-box {
        padding: 60px 63px 30px;
        @media (max-width: 767px) {
            padding: 30px 33px 20px;
        }
        .title {
            font-weight: $font-bold;
            margin-bottom: 23px;
            line-height: 1;
            @media (max-width: 767px) {
                font-size: 3vw !important;
            }
        }
    }
    .box-image + .box-product {
        padding: 30px 40px 25px;
        @media (max-width: 991px) {
            padding: 20px 20px 25px;
        }
        .product-wrapper .is-countdown .countdown-section {
            .countdown-amount {
                @media (min-width: 1440px) {
                    font-size: $body-font-size;
                }
            }
            .countdown-period {
                @media (min-width: 1440px) {
                    font-size: 8px;
                }
            }
        }
    }
}
/*=== Section lookbook ===*/
.lookbook-section {
    .lookbook-title {
        padding-top: 37px;
        @media (max-width: 991px) {
            padding-bottom: 30px !important;
        }
        @media (max-width: 767px) {
            padding-top: 20px;
            padding-bottom: 15px !important;
        }
        .heading {
            font-weight: 300;
            padding: 0;
            margin: 0;
            color: $body-general-color;
            &.big-size {
                @media (max-width: 991px) {
                    font-size: 30px !important;
                }
                @media (max-width: 767px) {
                    font-size: 20px !important;
                }
            }
        }
        .subheading {
            font-weight: $font-bold;
            line-height: 1.0;
            color: $body-general-color;
            &.big-size {
                @media (max-width: 991px) {
                    font-size: 40px !important;
                }
                @media (max-width: 767px) {
                    font-size: 30px !important;
                }
            }
            span {
                color: $main-color;
                text-decoration: underline;
            }
        }
    }
    .lookbook-content .row > div {
        margin-top: 20px;
        &:first-child {
            @media (max-width: 767px) {
                margin-top: 0;
            }
        }
        @media (min-width: 768px) {
            &.col-md-6:nth-child(-n+2) {
                margin-top: 0;
            }
            &.col-12:not(.col-md-6) {
                &:first-child {
                    margin-top: 0;
                }
                + .col-md-6 {
                    margin-top: 20px;
                }
            }
        }
    }
    .lookbook {
        position: relative;
        .lookbook-text {
            position: absolute;
            display: table;
            font-weight: $font-bold;
            z-index: 1;
            line-height: 1.0;
            .heading {
                @media (max-width: 767px) {
                    font-size: 4vw !important;
                }
            }
            .subheading {
                margin-top: 20px;
                @media (max-width: 767px) {
                    margin-top: 10px;
                    font-size: 2vw !important;
                }
            }
        }
        .pin {
            position: absolute;
            display: block;
            .open-pin {
                position: relative;
                display: block;
                width: 42px;
                height: 42px;
                line-height: 36px;
                font-size: 18px;
                font-weight: $font-bold;
                text-align: center;
                z-index: 1;
                cursor: pointer;
                @include border-radius-circle;
                @media (max-width: 767px) {
                    width: 24px;
                    height: 24px;
                    line-height: 20px;
                    font-size: 14px;
                    border: 2px solid $white;
                }
                &:hover {
                    text-decoration: underline;
                }
            }
            .pin-content {
                position: relative;
                z-index: 99;
                display: block;
                margin: -60px 0 0;
                @include margin-start(20px);
                width: 212px;
                background: $body-background-color;
                @include opacity(0);
                visibility: hidden;
                height: 0;
                @include box-shadow(0px 10px 20px 0px rgba(0, 0, 0, 0.1));
                @include transition(opacity 0.2s ease-in-out);
                @media (max-width: 767px) {
                    width: 200px;
                    margin: -48px 0 0;
                    @include margin-start(-88px);
                }
            }
            &.active {
                .open-pin {
                    z-index: 999;
                }
                .pin-content {
                    @include opacity(1);
                    visibility: visible;
                    height: auto;
                }
            }
        }
        .product-wrapper {
            .product-head .product-image img {
                @include transition(none);
            }
            .detail-deal {
                display: none;
            }
            .product-label {
                @include ab(n, n, n, auto, n, 0);
            }
            .product-content {
                text-align: center;
            }
            .product-button {
                @include flexbox;
                flex-wrap: wrap;
                align-items: center;
                justify-content: center;
                padding: 7px 0 0;
                > div {
                    @include icon-product();
                }
            }
            .product-name {
                padding-top: 15px;
            }
            .product-add-cart {
                background: $button-btn1;
                color: $button-btn1-txt;
                margin: 5px 0 0;
                padding: 9px 10px 10px;
                text-transform: uppercase;
                font-weight: $font-bold;
                a {
                    color: inherit;
                }
                .demo-icon {
                    @include margin-end(10px);
                    display: none;
                }
            }
        }
    }
}
/*=== Section two-column ===*/
.templateIndex .section-two-column .left-vertical-menu .navbar {
    height: auto;
    position: relative;
    @include transform(scaleY(1));
    @include transition(all 0.3s ease-in-out);
    &.opened {
        @include transform(scaleY(0));
        height: 0;
    }
}
.section-two-column {
    .position-left.padding-space {
        @media (max-width: 1199px) {
            padding-top: 20px !important;
        }
    }
    .two-column-content {
        background-size: cover;
        background-position: center center;
        background-repeat: no-repeat;
    }
    /* Left column */
    .left-vertical-menu {
        @include vertical-menu;
        .navbar {
            max-width: 297px;
            min-width: 100%;
        }
        .navbar-collapse {
            display: block;
        }
    }
    .two-column-left .banner-item {
        margin-top: 20px;
    }
    .block-product {
        border: 2px solid $progress-bar-secondary-border;
        padding: 20px 30px;
        h3 {
            font-size: $size-h4;
            font-weight: $font-bold;
        }
        .product-wrapper {
            margin-top: 16px;
        }
        .stock-available {
            text-align: center;
            font-size: 13px;
            display: block;
            margin-bottom: 4px;
        }
        .pieces-left {
            margin-top: 15px;
        }
        .progress {
            @include progress-bar($progress-bar-secondary-border);
        }
    }
    /* Main column */
    .two-column-main {
        @include flexbox;
        flex-wrap: wrap;
        margin:  0 -10px;
        > div {
            padding: 0 10px;
        }
    }
    /* Slideshow */
    .tc-swiper-container {
        position: relative;
        overflow: hidden;
        margin-top: 20px;
        max-width: 100%;
        @include flex(0 0 100%);
        @media (min-width: 768px) {
            max-height: 550px;
        }
        &:before {
            content: "";
            @include ab(absolute, block, 0, 0, n, n);
            background: #f3f3f3;
            width: 100%;
            height: 100%;
        }
        &.swiper-container-initialized:before {
            content: none;
        }
        .swiper-button-white {
            @media (min-width: 1025px) {
                @include opacity(0.2);
            }
        }
        .swiper-button-white {
            &.swiper-button-next {
                @include ab(n, n, n, n, n, 10px);
            }
            &.swiper-button-prev {
                @include ab(n, n, n, 10px, n, n);
            }
        }
    }
    .swiper-slide {
        position: relative;
        height: auto;
        margin: 0 auto;
        @include opacity(0);
        &.swiper-slide-active {
            @include opacity(1);
        }
        .img-mobile {
            @media (min-width: 768px) {
                display: none;
            }
        }
        .img-desk {
            object-fit: cover;
            font-family: 'object-fit:cover';
            width: 100%;
            max-height: 550px;
            @media (max-width: 767px) {
                display: none;
            }
        }
        .swiper-content {
            @include ab(absolute, block, 0, 0, 0 ,0);
            z-index: 99;
            .container-box {
                position: absolute;
                display: block;
            }
        }
    }
    /* Notice bar */
    .two-column-notice-bar .text-box {
        text-align: center;
        padding-top: 6px;
        padding-bottom: 6px;
    }
    /*  Banner */
    .two-column-image {
        margin-top: 20px;
        .banner-item .subtitle {
            @media (min-width: 1440px) {
                font-size: 35px;
            }
        }
    }
}
/*=== Section banner & text ===*/
.home-banner-text {
    .block-text {
        @include flexbox;
        flex-direction: column;
        justify-content: center;
        height: 100%;
        align-items: flex-start;
        @media (max-width: 767px) {
            margin-bottom: 15px;
            display: block;
            height: auto;
        }
    }
    .title {
        margin-bottom: 3px;
        &.big-size {
            @media (max-width: 1440px) {
                font-size: 20px !important;
            }
        }
    }
    .subtitle {
        line-height: 0.92;
        margin-bottom: 25px;
        &.big-size {
            @media (max-width: 1440px) {
                font-size: 25px !important;
            }
        }
    }
    .btn {
        padding: 2px 30px;
    }
}
/* Section search-widget-section */
.search-widget-section {
    .search-widget-inner {
        position: relative;
    }
    .search-form {
        @include ab(absolute, block, 50%, 50%, n, n);
        width: calc(100% - 30px);
        z-index: 99;
        @include translate(-50%, -50%);
        text-align: center;
        .title {
            font-weight: $font-bold;
            margin-bottom: 28px;
            @media (max-width: 767px) {
                font-size: $body-font-size !important;
                margin-bottom: 15px;
            }
        }
        form {
            width: 724px;
            height: 62px;
            margin: 0 auto;
            position: relative;
            @media (max-width: 991px) {
                width: 100%;
                height: 42px;
            }
            input {
                height: 100%;
            }
            button {
                background: $button-btn1;
                color: $button-btn1-txt;
                border: 1px solid $button-btn1-border;
                @include ab(absolute, block, 0, n, n, 0);
                height: 100%;
                width: 60px;
                @media (max-width: 991px) {
                    width: 50px;
                }
                i:before {
                    font-size: $font-small-icon;
                }
                &:hover {
                    background: $button-hover-btn1;
                    color: $button-hover-btn1-txt;
                    border: 1px solid $button-hover-btn1-border;
                }
            }
        }
    }
    .have-menu .search-form {
        top: 22%;
        @include translate(-50%, 0%);
        @media (max-width: 1199px) {
            top: 5%;
        }
    }
    &.layout-full .block-menu {
        @media (max-width: 991px) {
            max-width: 100%;
        }
    }
    .block-menu {      // follow Carparts home 2
        @include width-boxed;
        margin-top: -40px;
        position: relative;
        @media (min-width: 1200px) {
            @include padding-start(115px);
            @include padding-end(115px);
        }
        @media (max-width: 575px) {
            margin-top: 10px;
        }
        .item-list {
            background-repeat: no-repeat;
            background-position: right center;
            background-color: $body-background-color;
            padding: 30px 40px;
            border: 2px solid $main-color;
            @media (max-width: 1199px) {
                padding: 15px 20px;
            }
        }
        .title-wrapper {
            margin-bottom: 20px;
        }
        ul {
            padding-bottom: 10px;
            li {
                padding-bottom: 6px;
                a {
                    color: $body-link-color;
                    font-size: $body-font-size - 1px;
                }
            }
        }
    }
}
/*=== Section Custom content ===*/
.custom-content-width {
    .col-md-12 {
        padding: 0;
        margin: 0 auto;
    }
    .custom-content .custom-item {
        &.pad-left {
            @media (min-width: 768px) {
                @include padding-end(30px);
            }
            @media (min-width: 1200px) {
                @include padding-end(70px);
            }
        }
        &.pad-right {
            @media (min-width: 768px) {
                @include padding-start(30px);
            }
            @media (min-width: 1200px) {
                @include padding-start(70px);
            }
        }
    }
    &.wide .custom-content .custom-item {
        &.pad-left {
            @media (max-width: 767px) {
                padding: 0 25px;
            }
            @media (min-width: 768px) {
                @include padding-start(calc((100vw - (760px - 30px)) / 2));
            }
            @media (min-width: 992px) {
                @include padding-start(calc((100vw - (980px - 30px)) / 2));
            }
            @media (min-width: 1200px) {
                @include padding-start(calc((100vw - (1200px - 30px)) / 2));
            }
            @media (min-width: 1440px) {
                @include padding-start(calc((100vw - (#{$container-width} - 30px)) / 2));
            }
        }
        &.pad-right {
            @media (max-width: 767px) {
                padding: 0 25px;
            }
            @media (min-width: 768px) {
                @include padding-end(calc((100vw - (760px - 30px)) / 2));
            }
            @media (min-width: 992px) {
                @include padding-end(calc((100vw - (980px - 30px)) / 2));
            }
            @media (min-width: 1200px) {
                @include padding-end(calc((100vw - (1200px - 30px)) / 2));
            }
            @media (min-width: 1440px) {
                @include padding-end(calc((100vw - (#{$container-width} - 30px)) / 2));
            }
        }
    }
}
.custom-content-header {
    margin: 0 auto 3%;
}
.custom-item-inner-collection,
.custom-item-inner-html {
    display: block;
}
.custom-block-heading-text,
.custom-block-body-text {
    @include margin-start(auto);
    @include margin-end(auto);
}
.custom-block-heading-text {
    font-weight: $font-bold;
}
.custom-block-body-text p {
    font-size: inherit;
    margin: 0;
    line-height: 1.1;
}
.custom-content {
    @include flexbox();
    width: 100%;
    -ms-flex-align: stretch;
    -webkit-align-items: stretch;
    -moz-align-items: stretch;
    -ms-align-items: stretch;
    -o-align-items: stretch;
    align-items: stretch;
    -webkit-flex-wrap: wrap;
    -moz-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    width: auto;
    padding: 0;
    .placeholder-svg {
        fill: rgba($main-color, 0.35);
