}
.separator-heading-logo {
    max-width: 1200px;
    padding: 0 15px;
    margin: 0 auto;
    text-align: center;
    h2 {
        margin: 0 auto 11px;
    }
    span {
        display: block;
        &.separator-logo {
            margin: 0 auto 16px;
            &.logo-bt {
                margin: 10px auto 0;
            }
        }
    }
}
.separator-heading-line {
    margin: 0 auto;
    text-align: center;
    h2 {
        margin: 0 auto;
        font-family: $font-heading;
        @media (max-width: 991px) {
            font-size: $size-h1 !important;
        }
        @media (max-width: 991px) {
            font-size: $size-h1 - 4px !important;
        }
        &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
            line-height: 1.0;
            font-weight: $font-bold;
        }
        &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
            font-weight: 100;
        }
    }
    .separator-subtext {
        display: block;
        font-family: $font-heading;
        &.largest, &.largest-1, &.largest-2, &.largest-3, &.largest-4, &.largest-5 {
            line-height: 1.0;
            font-weight: $font-bold;
        }
        &.h1, &.h2, &.h3, &.h4, &.h5, &.h6 {
            font-weight: 100;
        }
    }
    .separator-sub-line {
        max-width: 100%;
        margin: 0 auto;
        overflow: hidden;
        border-top-width: 1px;
        border-bottom-width: 0;
        border-left-width: 0;
        border-right-width: 0;
        border-color: $body-border-color;
        &.sub-double-line {
            max-width: 80px;
            border-width: 1px;
        }
    }
    &.side-line {
        h2 {
            position: relative;
            margin: 0 auto;
            display: inline-block;
            padding: 0 50px;
            @media (max-width: 479px) {
                padding: 0 30px;
            }
            span {
                display: inline-block;
                padding: 0 20px;
                position: relative;
                z-index: 1;
                white-space: nowrap;
                background: $body-background-color;
            }
            &:after,
            &:before {
                content: '';
                height: 1px;
                width: 50%;
                border-top-width: 1px;
                border-bottom-width: 0;
                border-left-width: 0;
                border-right-width: 0;
                @include translate(0, -50%);
            }
            &:before {
                @include ab(absolute, block, 50%, 50%, n, n);
            }
            &:after {
                @include ab(absolute, block, 50%, n, n, 50%);
            }
        }
        &.line-solid h2 {
            &:after,
            &:before {
                border-style: solid;
            }
        }
        &.line-dotted h2 {
            &:after,
            &:before {
                border-style: dotted;
            }
        }
        &.line-dashed h2 {
            &:after,
            &:before {
                border-style: dashed;
            }
        }
        .separator-logo {
            margin: 8px auto 13px;
            display: block;
        }
        .separator-subtext {
            margin: 8px auto 0;
        }
    }
    &.left-line {
        text-align: start;
        h2 {
            position: relative;
            padding: 0;
            display: block;
            &:after {
                content: none !important;
            }
            &:before {
                content: '';
                height: 1px;
                width: 100%;
                border-width: 1px;
                @include ab(absolute, block, 50%, n, n, 0);
                @include translate(0, -50%);
            }
            span {
                display: inline-block;
                padding: 0;
                @include padding-end(8%);
                position: relative;
                background: $body-background-color;
            }
        }
        &.line-solid h2:before {
            border-style: solid;
        }
        &.line-dotted h2:before {
            border-style: dotted;
        }
        &.line-dashedh2:before {
            border-style: dashed;
        }
    }
    &.right-line {
        text-align: end;
        h2 {
            position: relative;
            padding: 0;
            display: block;
            &:after {
                content: none !important;
            }
            &:before {
                content: '';
                height: 1px;
                width: 100%;
                border-width: 1px;
                @include ab(absolute, block, 50%, 0, n, n);
                @include translate(0, -50%);
            }
            span {
                display: inline-block;
                padding: 0;
                @include padding-start(8%);
                position: relative;
                background: $body-background-color;
            }
        }
        &.line-solid h2:before {
            border-style: solid;
        }
        &.line-dotted h2:before {
            border-style: dotted;
        }
        &.line-dashed h2:before {
            border-style: dashed;
        }
    }
    &.line-solid .separator-sub-line {
        border-style: solid;
    }
    &.line-dotted .separator-sub-line {
        border-style: dotted;
    }
    &.line-dashed .separator-sub-line {
        border-style: dashed;
    }
}
