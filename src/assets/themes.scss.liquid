        }
        &.bottom-center .text-box {
            text-align: center;
            @include translate(-50%, 0);
            @include ab(n, n, n, 50%, 11%, n);
            @media (max-width: 1200px) {
                @include ab(n, n, n, n, 5%, n);
            }
        }
        &.bottom-right .text-box {
