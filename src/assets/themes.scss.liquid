                @media (max-width: 767px) {
                    width: 50%;
                }
            }
            &.owl-loaded {
                margin: 0;
                .product-grid-item {
                    width: 100%;
                    padding: 0;
                }
            }
            .owl-nav {
                background: transparent;
            }
        }
    }
