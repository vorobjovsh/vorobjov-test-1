        }
        .text {
            font-size: $body-font-size;
            text-align: center;
            font-weight: $font-bold;
            padding-bottom: 15px;
        }
        .product-grid-carousel-v3 {
            @include flexbox;
            flex-wrap: wrap;
            margin: 0 -10px;
            .product-grid-item {
                width: 33.33333%;
